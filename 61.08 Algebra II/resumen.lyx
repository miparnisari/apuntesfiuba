\begin_layout Subsection
Optimización restringida 
\end_layout

\begin_layout Standard

\series bold
Teorema de Rayleigh
\series default
: sea la forma cuadrática 
\begin_inset Formula $Q\left(x\right)=x^{T}Ax$
\end_inset

, con 
\begin_inset Formula $A$
\end_inset

 simétrica.
 Se verifica: 
\begin_inset Formula 
\[
\lambda_{min}(A)\leq\frac{Q(x)}{\left\Vert x\right\Vert ^{2}}\leq\lambda_{max}(A)
\]

\end_inset

 Sea extremar una forma cuadrática 
\begin_inset Formula $f:\mathbb{R}^{n}\to\mathbb{R}$
\end_inset

, 
\begin_inset Formula $f\left(x\right)=x^{T}Ax$
\end_inset

 (
\begin_inset Formula $A$
\end_inset

 simétrica), sujeto a la restricción 
\begin_inset Formula $\left|x\right|=\alpha$
\end_inset

.
 El máximo de 
\begin_inset Formula $f$
\end_inset

 es 
\begin_inset Formula $\lambda_{max}(A).\alpha^{2}$
\end_inset

 y se alcanza en 
\begin_inset Formula $M=\{x\in S_{\lambda_{max}}\left(A\right):\left|x\right|=\alpha\}$
\end_inset

.
 El mínimo de 
\begin_inset Formula $f$
\end_inset

 es 
\begin_inset Formula $\lambda_{min}(A).\alpha^{2}$
\end_inset

 y se alcanza en 
\begin_inset Formula $m=\{x\in S_{\lambda_{min}}\left(A\right):\left|x\right|=\alpha\}$
\end_inset

.
\end_layout

\begin_layout Standard
Sea extremar una forma cuadrática 
\begin_inset Formula $f:\mathbb{R}^{n}\to\mathbb{R}$
\end_inset

, 
\begin_inset Formula $f\left(x\right)=x^{T}Ax$
\end_inset

 (
\begin_inset Formula $A$
\end_inset

 simétrica), sujeto a la restricción 
\begin_inset Formula $x^{T}Bx=\alpha^{2}$
\end_inset

, y sea 
\begin_inset Formula $B$
\end_inset

 definida positiva tal que 
\begin_inset Formula $B=P_{B}D_{B}P_{B}^{T}$
\end_inset

.
 Mediante un cambio de variable 
\begin_inset Formula $y=\sqrt{D_{B}}P_{B}^{T}x$
\end_inset

 (
\begin_inset Formula $x=\sqrt{D_{B}^{-1}}P_{B}y$
\end_inset

), esto es equivalente a extremar 
\begin_inset Formula $g\left(y\right)=y^{T}\left(\sqrt{D_{B}^{-1}}P_{B}^{T}AP_{B}\sqrt{D_{B}^{-1}}\right)y$
\end_inset

 sujeto a la restricción 
\begin_inset Formula $\left|y\right|=\alpha$
\end_inset

.
 Entonces: 
\begin_inset Formula $\max g\left(y\right)=\max f(x)$
\end_inset

, y 
\begin_inset Formula $\min g\left(y\right)=\min f(x)$
\end_inset

.
 Los 
\begin_inset Formula $x$
\end_inset

 en donde se alcanza ese extremo se hallan realizando la cuenta 
\begin_inset Formula $x=P_{B}y$
\end_inset

.
\end_layout

\begin_layout Subsection
DVS
\end_layout

\begin_layout Standard

\series bold
Valores singulares
\series default
: 
\begin_inset Formula $VS\left(A\right)=\sqrt{\lambda_{i}}\:\forall\lambda_{i}\in\sigma(A^{T}A)$
\end_inset

.
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $A$
\end_inset

 una matriz de 
\begin_inset Formula $m\times n$
\end_inset

 con rango 
\begin_inset Formula $r$
\end_inset

.
 Entonces existe una matriz 
\begin_inset Formula $\Sigma$
\end_inset

, y existen una matriz U ortogonal de 
\begin_inset Formula $m\times m$
\end_inset

 y una matriz 
\begin_inset Formula $V$
\end_inset

 ortogonal de 
\begin_inset Formula $n\times n$
\end_inset

 tales que 
\begin_inset Formula $A=U\Sigma V^{T}$
\end_inset

, donde: 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\Sigma\in\mathbb{R}^{m\times n}$
\end_inset

 es tal que 
\begin_inset Formula $\Sigma=\left[\begin{array}{cc}
D & 0\\
0 & 0
\end{array}\right]$
\end_inset

, y las entradas diagonales de D son los primeros 
\begin_inset Formula $r$
\end_inset

 valores singulares de A, ordenados en forma descendente: 
\begin_inset Formula $\sigma_{i}\ge\sigma_{2}\ge\ldots\ge\sigma_{r}>0$
\end_inset

.
 
\end_layout

\begin_layout Itemize
\begin_inset Formula $V\in\mathbb{R}^{n\times n}$
\end_inset

 es una matriz cuyas columnas son una BON de autovectores 
\begin_inset Formula $\left\{ v_{1},v_{2},\ldots,v_{n}\right\} $
\end_inset

 asociados a los autovalores de 
\begin_inset Formula $A^{T}A$
\end_inset

.
 
\end_layout

\begin_layout Itemize
\begin_inset Formula $U\in\mathbb{R}^{m\times m}$
\end_inset

 es una matriz cuyas primeras 
\begin_inset Formula $r$
\end_inset

 columnas son los vectores 
\begin_inset Formula $\frac{Av_{1}}{\sigma_{1}},\ldots,\frac{Av_{r}}{\sigma_{r}}$
\end_inset

.
 Las otras columnas se obtienen completando una BON para 
\begin_inset Formula $\mathbb{R}^{m}$
\end_inset

.
 Las columnas de U son autovectores de 
\begin_inset Formula $AA^{T}$
\end_inset

.
 
\end_layout

\begin_layout Subsection
Aplicaciones de las DVS
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $A\in\mathbb{K}^{m\times n},A=\left[\begin{array}{ccc}
| &  & |\\
u_{1} & \cdots & u_{m}\\
| &  & |
\end{array}\right]\left[\begin{array}{cc}
D & 0\\
\vdots & 0\\
0 & 0
\end{array}\right]\left[\begin{array}{ccc}
| &  & |\\
v_{1} & \cdots & v_{n}\\
| &  & |
\end{array}\right]^{T}$
\end_inset

.
 Si 
\begin_inset Formula $rg\left(A\right)=r$
\end_inset

 entonces: 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left\{ u_{1},u_{2},\ldots,u_{r}\right\} $
\end_inset

 es BON de 
\begin_inset Formula $Col(A)$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\{u_{r+1},\ldots,u_{m}\}$
\end_inset

 es BON de 
\begin_inset Formula $\left[Col\left(A\right)\right]^{\bot}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\{v_{1},v_{2},\ldots,v_{r}\}$
\end_inset

 es BON de 
\begin_inset Formula $Fil\left(A\right)$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\{v_{r+1},\ldots,v_{n}\}$
\end_inset

 es BON de 
\begin_inset Formula $\left[Fil\left(A\right)\right]^{\bot}$
\end_inset


\end_layout

\begin_layout Subsection
Propiedades de las DVS
\end_layout

\begin_layout Itemize
\begin_inset Formula $rg\left(A\right)=rg\left(\Sigma\right)=rg\left(\Sigma^{T}\right)=\#VS\left(A\right)_{positivos}$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $A\in\mathbb{R}^{n\times n}$
\end_inset

 es inversible 
\begin_inset Formula $\Longleftrightarrow$
\end_inset

 
\begin_inset Formula $A$
\end_inset

 tiene 
\begin_inset Formula $n$
\end_inset

 VS positivos 
\end_layout

\begin_layout Itemize
\begin_inset Formula $VS\left(A\right)_{positivos}=VS\left(A^{T}\right)_{positivos}$
\end_inset

 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $A\in\mathbb{R}^{n\times n},\left|det(A)\right|=\prod_{i=1}^{n}VS_{i}(A)$
\end_inset

 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $A$
\end_inset

 es cuadrada y definida positiva, 
\begin_inset Formula $\sigma\left(A\right)\equiv VS(A)$
\end_inset

 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $A$
\end_inset


\begin_inset Formula $\sim$
\end_inset


\begin_inset Formula $B$
\end_inset

, entonces 
\begin_inset Formula $VS\left(A\right)\equiv VS(B)$
\end_inset

 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $B$
\end_inset

 es ortogonal, 
\begin_inset Formula $A$
\end_inset

, 
\begin_inset Formula $AB$
\end_inset

 y 
\begin_inset Formula $BA$
\end_inset

 tienen los mismos valores singulares 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $A$
\end_inset

 es cuadrada y simétrica, entonces 
\begin_inset Formula $VS_{i}\left(A\right)=\left|\lambda_{i}(A)\right|$
\end_inset

 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $A$
\end_inset

 tiene filas BON, los valores singulares no nulos de 
\begin_inset Formula $A$
\end_inset

 son 1.
 Si 
\begin_inset Formula $A$
\end_inset

 tiene columnas BON, los valores singulares de 
\begin_inset Formula $A$
\end_inset

 son 1 
\end_layout

\begin_layout Itemize
La matriz 
\begin_inset Formula $A^{T}A$
\end_inset

 (matriz de 
\emph on
Gram
\emph default
 de 
\begin_inset Formula $A$
\end_inset

) es siempre simétrica y semi definida positiva, con lo cual nunca tendrá
 valores singulares negativos.
 Será definida positiva cuando 
\begin_inset Formula $A$
\end_inset

 tenga columnas LI 
\end_layout

\begin_layout Itemize
Sea 
\begin_inset Formula $T:\mathbb{R}^{m}\to\mathbb{R}^{n}$
\end_inset

 una transformación lineal tal que 
\begin_inset Formula $T\left(x\right)=Ax$
\end_inset

.
 Sea la forma cuadrática 
\begin_inset Formula $f\left(x\right)=\left|T\left(x\right)\right|^{2}=x^{T}\left(A^{T}A\right)x$
\end_inset

.
 Entonces: 
\end_layout

\begin_deeper
\begin_layout Enumerate
El máximo de 
\begin_inset Formula $f(x)$
\end_inset

 sujeto a 
\begin_inset Formula $\left|x\right|=1$
\end_inset

 es 
\begin_inset Formula $\lambda_{max}\left(A^{T}A\right)$
\end_inset

.
 Entonces el máximo de 
\begin_inset Formula $\left|T(x)\right|$
\end_inset

 es 
\begin_inset Formula $VS_{max}(A)$
\end_inset

 y se alcanza en 
\begin_inset Formula $M=\{x\in\mathbb{R}^{m}:x\in S_{\lambda_{max}}\left(A^{T}A\right),\left|x\right|=1\}$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
El mínimo de 
\begin_inset Formula $f(x)$
\end_inset

 sujeto a 
\begin_inset Formula $\left|x\right|=1$
\end_inset

 es 
\begin_inset Formula $\lambda_{min}\left(A^{T}A\right)$
\end_inset

.
 Entonces el mínimo de 
\begin_inset Formula $\left|T(x)\right|$
\end_inset

 es 
\begin_inset Formula $VS_{min}(A)$
\end_inset

 y se alcanza en 
\begin_inset Formula $m=\{x\in\mathbb{R}^{m}:x\in S_{\lambda_{min}}\left(A^{T}A\right),\left|x\right|=1\}$
\end_inset

.
 
\end_layout

\end_deeper
\begin_layout Subsection
DVS reducida con matrices de proyección y pseudoinversa
\end_layout

\begin_layout Standard

\series bold
DVS reducida:
\series default
 Sea 
\begin_inset Formula $A\in\mathbb{R}^{n\times m}$
\end_inset

.
 Si 
\begin_inset Formula $A=U\Sigma V^{T}$
\end_inset

, y 
\begin_inset Formula $rg\left(A\right)=r$
\end_inset

, entonces una DVS reducida de 
\begin_inset Formula $A$
\end_inset

 es 
\begin_inset Formula $A=U_{r}\Sigma_{r}V_{r}^{T}$
\end_inset

 siendo 
\begin_inset Formula $U_{r}\in\mathbb{K}^{n\times r}$
\end_inset

, 
\begin_inset Formula $\Sigma_{r}\in\mathbb{K}^{r\times r}$
\end_inset

, 
\begin_inset Formula $V_{r}^{T}\in\mathbb{K}^{r\times m}$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
Pseudoinversa de Moore-Penrose de A
\series default
: 
\begin_inset Formula $A^{+}=V_{r}\Sigma_{r}^{-1}U_{r}^{T}$
\end_inset


\end_layout

\begin_layout Standard

\bar under
Propiedades
\bar default
: 
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $A\in\mathbb{R}^{n\times m}:$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $A^{+}=A^{\#}$
\end_inset

cuando 
\begin_inset Formula $rg\left(A\right)=m$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $A^{+}A=V_{r}V_{r}^{T}=P_{Fil\left(A\right)}$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $AA^{+}=U_{r}U_{r}^{T}=P_{Col(A)}$
\end_inset

 
\end_layout

\begin_layout Section
Ecuaciones Diferenciales
\end_layout

\begin_layout Subsection
Independencia lineal y Wronskiano
\end_layout

\begin_layout Standard

\series bold
Matriz de Wronski
\series default
: Sea 
\begin_inset Formula $A=\left\{ f_{1},f_{2},\ldots,f_{q}\right\} $
\end_inset

 funciones definidas en un intervalo 
\begin_inset Formula $I\subset\mathbb{R}$
\end_inset

, a valores en 
\begin_inset Formula $\mathbb{C}$
\end_inset

, con derivada hasta el orden 
\begin_inset Formula $(q-1)$
\end_inset

 continua en 
\begin_inset Formula $I$
\end_inset

.
 La matriz de wronski de 
\begin_inset Formula $A$
\end_inset

 es, para cada 
\begin_inset Formula $x\in I$
\end_inset

: 
\begin_inset Formula 
\[
Mw_{A}\left(x\right)=\left[\begin{array}{ccc}
f_{1}(x) & \cdots & f_{q}(x)\\
f_{1}^{'}(x) & \cdots & f_{q}^{'}(x)\\
f_{1}^{\left(q-1\right)}(x) & \cdots & f_{q}^{\left(q-1\right)}(x)
\end{array}\right]
\]

\end_inset

 
\series bold
Wronskian
\series default
o: 
\begin_inset Formula $w_{A}\left(t\right)=det(Mw_{A})$
\end_inset


\end_layout

\begin_layout Standard

\bar under
Propiedades
\bar default
:
\end_layout

\begin_layout Itemize
Si existe un 
\begin_inset Formula $x_{0}\in I$
\end_inset

 tal que 
\begin_inset Formula $w_{A}(x_{0})\ne0$
\end_inset

, entonces las funciones 
\begin_inset Formula $f_{1},f_{2},\ldots,f_{q}$
\end_inset

 son LI.
 
\end_layout

\begin_layout Itemize
Si un conjunto es LD en un 
\begin_inset Formula $I$
\end_inset

, su wronskiano es la función nula.
 La recíproca es falsa; es verdadera solo si las funciones que componen
 el wronskiano son soluciones de una ED lineal de orden superior.
 
\end_layout

\begin_layout Itemize
La derivada del wronskiano es el determinante obtenido derivando la última
 fila.
 La derivada del wronskiano es la suma de 
\begin_inset Formula $q$
\end_inset

 determinantes.
 
\end_layout

\begin_layout Subsection
Identidad de Abel
\end_layout

\begin_layout Standard
Sea la ecuación diferencial 
\begin_inset Formula $y(x)^{(n)}+a_{n-1}\cdot y(x)^{\left(n-1\right)}+\ldots+a_{1}\cdot y'\left(x\right)+a_{0}\cdot y\left(x\right)=0$
\end_inset

 en un intervalo 
\begin_inset Formula $I\subset\mathbb{R}$
\end_inset

, sea 
\begin_inset Formula $S=\{y_{1},y_{2},\ldots,y_{n}\}$
\end_inset

 el conjunto de las soluciones de ED, y sea 
\begin_inset Formula $W_{S}$
\end_inset

 el wronskiano de este conjunto.
 Entonces se verifica que: 
\begin_inset Formula 
\[
W_{S}^{'}(t)=-a_{n-1}\cdot W_{S}(t)
\]

\end_inset


\end_layout

\begin_layout Subsection
Ecuaciones diferenciales lineales
\end_layout

\begin_layout Subsubsection
Condición de existencia y unicidad de un PVI
\end_layout

\begin_layout Standard
Sea el problema 
\begin_inset Formula $\left\{ \begin{array}{c}
a_{n}y\left(x\right)^{(n)}+\ldots+a_{1}y'\left(x\right)+a_{0}y(x)=f(x)\\
y\left(x_{0}\right)=y_{0}
\end{array}\right.$
\end_inset


\end_layout

\begin_layout Standard
La condición de existencia y unicidad de la solución del PVI es: 
\end_layout

\begin_layout Itemize
ED normal en 
\begin_inset Formula $I$
\end_inset

: 
\begin_inset Formula $a_{n}\ne0\:\forall x\in I$
\end_inset

.
 
\end_layout

\begin_layout Itemize
\begin_inset Formula $x_{0}\in I$
\end_inset

.
 
\end_layout

\begin_layout Subsubsection
Variables separables: 
\begin_inset Formula $y'=\frac{f(x)}{g(x)}$
\end_inset

 con 
\begin_inset Formula $y=y(x)$
\end_inset


\end_layout

\begin_layout Standard
Separamos la ecuación en 
\begin_inset Formula $g\left(x\right)\cdot dy=f\left(x\right)\cdot dx$
\end_inset

 e integrando ambos miembros se tiene 
\begin_inset Formula $G\left(y\right)=F\left(x\right)+c$
\end_inset

.
 
\end_layout

\begin_layout Subsubsection
Homogéneas: 
\begin_inset Formula $y'=f(x,y)$
\end_inset

 con 
\begin_inset Formula $f\left(tx,ty\right)=f(x,y)$
\end_inset

 
\end_layout

\begin_layout Standard
Hacemos un cambio de variable 
\begin_inset Formula $y=zx$
\end_inset

 (donde 
\begin_inset Formula $z=z\left(x\right)$
\end_inset

).
 Entonces 
\begin_inset Formula $y'=z+xz'$
\end_inset

.
 
\end_layout

\begin_layout Subsubsection
Lineales de 1
\begin_inset Formula $^{er}$
\end_inset

 orden
\end_layout

\begin_layout Standard
Obtenemos primero la solución general de la homogénea (
\begin_inset Formula $y_{h}$
\end_inset

) y luego una particular de la no homogénea (
\begin_inset Formula $y_{p}$
\end_inset

).
 La solución buscada es 
\begin_inset Formula $y_{G}=y_{H}+y_{P}$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
Solución de la ecuación homogénea asociada (
\begin_inset Formula $y'+p\left(x\right)y=0$
\end_inset

): es de variables separables.
 Una solución es 
\begin_inset Formula $y_{H}\left(x\right)=e^{-\int p(x)dx}$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
Solución de la no homogénea: una solución se obtiene multiplicando toda
 la ecuación por el factor integrante de Lagrange: 
\begin_inset Formula $u\left(v\right)=e^{\int p\left(x\right)dx}$
\end_inset

.
 Entonces, la ecuación a resolver será 
\begin_inset Formula $\left[u\left(v\right).y\right]'=u\left(v\right).q(x)$
\end_inset

 
\end_layout

\begin_layout Subsubsection
Diferencial exacta: 
\begin_inset Formula $P(x,y)dx+Q(x,y)dy=0$
\end_inset


\end_layout

\begin_layout Standard
Es diferencial exacta si existe 
\begin_inset Formula $f(x,y)$
\end_inset

 tal que 
\begin_inset Formula $df=P\left(x,y\right)dx+Q\left(x,y\right)dy$
\end_inset

, es decir si 
\begin_inset Formula $\frac{\partial f}{\partial x}=P(x,y)$
\end_inset

 y 
\begin_inset Formula $\frac{\partial f}{\partial y}=Q(x,y)$
\end_inset

.
 En ese caso, la solución general es 
\begin_inset Formula $f\left(x,y\right)=C$
\end_inset

.
 Se cumple que la ecuación anterior es diferencial exacta si y solo si 
\begin_inset Formula $\frac{\partial P}{\partial y}=\frac{\partial Q}{\partial x}$
\end_inset

.
\end_layout

\begin_layout Standard
Se dice además que 
\begin_inset Formula $\mu(x,y)$
\end_inset

 es un factor integrante de la ecuación 
\begin_inset Formula $P\left(x,y\right)dx+Q\left(x,y\right)dy=0$
\end_inset

 si al multiplicar la ecuación por 
\begin_inset Formula $\mu(x,y)$
\end_inset

 la ecuación resultante es diferencial exacta.
 
\end_layout

\begin_layout Subsubsection
Lineales homogéneas de orden superior con coeficientes constantes: 
\begin_inset Formula $a_{n}(t)y^{(n)}+...+a_{1}y'+a_{0}y=0\,\forall t\in I$
\end_inset

 (I)
\end_layout

\begin_layout Standard

\series bold
Polinomio característico de (I)
\series default
: 
\begin_inset Formula $p\left(\lambda\right)=\sum_{i=0}^{n}a_{i}\lambda_{i}^{n}$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Espectro de (I)
\series default
: 
\begin_inset Formula $\sigma\left(p\right)=\{\lambda\in\mathbb{C}:\: p\left(\lambda\right)=0\}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $y_{H}\left(t\right)=t^{k}e^{\lambda t}$
\end_inset

 es una solución de la ED si 
\begin_inset Formula $\lambda\in\sigma(p)$
\end_inset

, multiplicidad 
\begin_inset Quotes eld
\end_inset


\begin_inset Formula $m$
\end_inset


\begin_inset Quotes erd
\end_inset

, 
\begin_inset Formula $k=0,1,\ldots,m-1$
\end_inset

.
\end_layout

\begin_layout Standard
Si la ED es de coeficientes constantes reales, las raíces del polinomio
 característico aparecerán conjugadas.
 Esto es, 
\begin_inset Formula $\lambda_{1,2}=\alpha\pm\beta i$
\end_inset

.
 Luego, 
\begin_inset Formula $y_{1}=e^{\alpha t}\left(\cos\left(\beta t\right)+i.sen\left(\beta t\right)\right)$
\end_inset

 e 
\begin_inset Formula $y_{2}=e^{\alpha t}\left(\cos\left(\beta t\right)-i.sen\left(\beta t\right)\right)$
\end_inset

.
 Entonces, 
\begin_inset Formula $gen\left\{ y_{1},y_{2}\right\} =gen\left\{ e^{\alpha t}\cos\left(\beta t\right),e^{\alpha t}sen\left(\beta t\right)\right\} $
\end_inset

.
 
\end_layout

\begin_layout Subsubsection
Lineales no homogéneas de orden superior con coeficientes constantes: 
\begin_inset Formula $a_{n}y^{(n)}+...+a_{1}y'+a_{0}y=f(x)$
\end_inset

 
\end_layout

\begin_layout Standard
La solución general es de la forma 
\begin_inset Formula $y_{G}=y_{P}+y_{H}$
\end_inset

, donde 
\begin_inset Formula $y_{H}$
\end_inset

 se obtiene como antes, e 
\begin_inset Formula $y_{P}$
\end_inset

 se obtiene mediante alguno de estos métodos:
\end_layout

\begin_layout Standard

\series bold
Método de variación de parámetros
\series default
: aplicable en cualquier caso.
 
\end_layout

\begin_layout Standard
\begin_inset Formula $y_{P}\left(t\right)=\sum_{i=1}^{n}u_{i}\left(x\right)\cdot y_{i}(x)$
\end_inset

, siendo 
\begin_inset Formula $y_{i}(x)$
\end_inset

 las soluciones de 
\begin_inset Formula $y_{H}$
\end_inset

, y 
\begin_inset Formula $u_{i}\left(x\right)$
\end_inset

 las funciones que satisfacen 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left[\begin{array}{cccc}
y_{1} & y_{2} & \ldots & y_{n}\\
y_{1}^{'} & y_{2}^{'} & \ldots & y_{n}^{'}\\
\vdots & \vdots & \ddots & \vdots\\
y_{1}^{\left(n\right)} & y_{2}^{(n)} & \ldots & y_{n}^{\left(n\right)}
\end{array}\right]\cdot\left[\begin{array}{c}
u_{1}^{'}\\
u_{2}^{'}\\
\vdots\\
u_{n}^{'}
\end{array}\right]=\left[\begin{array}{c}
0\\
0\\
\vdots\\
\frac{f\left(x\right)}{a_{n}}
\end{array}\right]
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Método de coeficientes indeterminados
\series default
: aplicable cuando 
\begin_inset Formula $f(x)$
\end_inset

 es exponencial, polinomio, seno y coseno, siendo 
\begin_inset Formula $a_{2}y''+a_{1}y'+a_{0}y=f(x)$
\end_inset

 con 
\begin_inset Formula $f\left(x\right)=p_{1}\left(x\right).e^{m_{1}x}+\ldots+p_{k}\left(x\right).e^{m_{k}x}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $y_{P}\left(t\right)=q_{1}\left(x\right).e^{m_{1}x}+\ldots+q_{k}\left(x\right).e^{m_{k}x}.$
\end_inset


\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $e^{m_{k}x}$
\end_inset

 no es solución de la EDO homogénea asociada (i.e.
 
\begin_inset Formula $m_{k}$
\end_inset

 no es raíz de 
\begin_inset Formula $a_{2}\lambda^{2}+a_{1}\lambda+a_{0}=0$
\end_inset

), 
\begin_inset Formula $q_{k}$
\end_inset

 es un polinomio del grado de 
\begin_inset Formula $p_{k}$
\end_inset

 con coeficientes a determinar.
 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $e^{m_{k}x}$
\end_inset

 si es solución de la EDO homogénea asociada (i.e.
 
\begin_inset Formula $m_{k}$
\end_inset

 sí es raíz de 
\begin_inset Formula $a_{2}"\lambda^{2}+a_{1}"\lambda+a_{0}=0$
\end_inset

), 
\begin_inset Formula $q_{k}$
\end_inset

 es un polinomio de un grado mayor que 
\begin_inset Formula $p_{k}$
\end_inset

 con coeficientes a determinar.
 
\end_layout

\begin_layout Itemize
Una vez armada la 
\begin_inset Formula $y_{P}$
\end_inset

 se reemplaza en la ED original, e igualando los términos semejantes se
 hallan los coeficientes indeterminados.
 
\end_layout

\begin_layout Section
Sistemas de Ecuaciones Diferenciales Lineales
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{cases}
x'_{t}=a_{11}x+a_{12}y+b_{1}\\
y'_{t}=a_{21}x+a_{22}y+b_{2}
\end{cases}\Longrightarrow\left[\begin{array}{c}
x'\\
y'
\end{array}\right]=\left[\begin{array}{cc}
a_{11} & a_{12}\\
a_{21} & a_{22}
\end{array}\right]\left[\begin{array}{c}
x\\
y
\end{array}\right]+\left[\begin{array}{c}
b_{1}\\
b_{2}
\end{array}\right]\Longrightarrow X'=AX+B
\]

\end_inset


\end_layout

\begin_layout Subsection
Sistemas homogéneos con 
\begin_inset Formula $\mathbf{A}$
\end_inset

 diagonalizable
\end_layout

\begin_layout Standard
La solución de 
\begin_inset Formula $X'=AX$
\end_inset

 (
\begin_inset Formula $A\in\mathbb{K}^{n\times n}$
\end_inset

), con 
\begin_inset Formula $\lambda_{i}$
\end_inset

 autovalor de 
\begin_inset Formula $A$
\end_inset

 y 
\begin_inset Formula $v_{i}$
\end_inset

 autovector de 
\begin_inset Formula $A$
\end_inset

 asociado a 
\begin_inset Formula $\lambda_{i}$
\end_inset

, es: 
\begin_inset Formula 
\[
X=\sum_{i=1}^{n}c_{i}v_{i}e^{\lambda_{i}t}=\underbrace{\left[\begin{array}{ccc}
| &  & |\\
v_{1}e^{\lambda_{1}t} & \cdots & v_{n}e^{\lambda_{n}t}\\
| &  & |
\end{array}\right]}_{\varphi(t)\in\mathbb{K}^{n\times n}}\underbrace{\left[\begin{array}{c}
c_{1}\\
\vdots\\
c_{n}
\end{array}\right]}_{C}
\]

\end_inset


\end_layout

\begin_layout Subsection
Sistemas no homogéneos con 
\begin_inset Formula $\mathbf{A}$
\end_inset

 diagonalizable
\end_layout

\begin_layout Standard
Sea el sistema 
\begin_inset Formula $X'=AX+B$
\end_inset

.
 La solución es 
\begin_inset Formula $X_{G}=X_{H}+X_{P}$
\end_inset

 con: 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $X_{H}=\sum_{i=1}^{n}c_{i}v_{i}e^{\lambda_{i}t}=\varphi\left(t\right)\cdot C$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $X_{P}=\varphi(t)\cdot u(t)$
\end_inset

 siendo 
\begin_inset Formula $u(t)$
\end_inset

 tal que 
\begin_inset Formula $\varphi\left(t\right)\cdot u'\left(t\right)=B$
\end_inset

 
\end_layout

\begin_layout Subsection
Sistemas homogéneos con 
\begin_inset Formula $\mathbf{A}$
\end_inset

 no diagonalizable
\end_layout

\begin_layout Standard
Sea el sistema 
\begin_inset Formula $X'=AX$
\end_inset

 con 
\begin_inset Formula $A$
\end_inset

 no diagonalizable, proponemos una factorización de la forma 
\begin_inset Formula $A=PJP^{-1}$
\end_inset

, donde 
\begin_inset Formula $J\in\mathbb{C}^{n\times n}$
\end_inset

 es la 
\emph on
matriz de Jordan
\emph default
 de 
\begin_inset Formula $A$
\end_inset

 que tiene la siguiente estructura en bloques: 
\begin_inset Formula $J=\left[\begin{array}{cccc}
J_{1} & 0 & 0 & 0\\
0 & J_{2} & 0 & 0\\
0 & 0 & \ddots & 0\\
0 & 0 & 0 & J_{l}
\end{array}\right]$
\end_inset

 donde cada bloque 
\begin_inset Formula $J_{i}$
\end_inset

 es una matriz 
\begin_inset Formula $k_{i}\times k_{i}$
\end_inset

 de la forma 
\begin_inset Formula $J_{i}=\left[\begin{array}{cccc}
\lambda_{i} & 1 & 0 & 0\\
0 & \lambda_{i} & 1 & 0\\
\vdots & \vdots & \ddots & 1\\
0 & 0 & \cdots & \lambda_{i}
\end{array}\right]$
\end_inset

 para algún autovalor 
\begin_inset Formula $\lambda_{i}$
\end_inset

de 
\begin_inset Formula $A$
\end_inset

.
 (Dado un autovalor 
\begin_inset Formula $\lambda_{i}$
\end_inset

, su multiplicidad geométrica es el número de bloques de Jordan correspondientes
 a 
\begin_inset Formula $\lambda_{i}$
\end_inset

, y su multiplicidad algebraica es la suma de los tamaños de los bloques
 correspondientes a ese autovalor.)
\end_layout

\begin_layout Standard
Luego: 
\begin_inset Formula $X'=AX\overset{X=PY}{\longmapsto}PY'=PJP^{-1}PY\longmapsto Y^{'}=JY$
\end_inset

.
 Resolvemos este sistema y la solución general del problema se expresará
 como 
\begin_inset Formula $X\left(t\right)=PY(t)$
\end_inset

.
\end_layout

\begin_layout Subsubsection
Caso 
\begin_inset Formula $\mathbf{A}\in\mathbb{R}^{\mathbf{2}\mathbf{\times}\mathbf{2}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $A$
\end_inset

 necesariamente posee un autovalor doble 
\begin_inset Formula $\lambda\in\mathbb{R}$
\end_inset

 de multiplicidad geométrica 1, con lo cual la matriz 
\begin_inset Formula $J$
\end_inset

 posee un solo bloque correspondiente a 
\begin_inset Formula $\lambda$
\end_inset

: 
\begin_inset Formula 
\[
J=\left[\begin{array}{cc}
\lambda & 1\\
0 & \lambda
\end{array}\right]
\]

\end_inset

 Respecto de la matriz 
\begin_inset Formula $P=\left[\begin{array}{cc}
v_{1} & v_{2}\end{array}\right]$
\end_inset

 deber ser inversible y 
\begin_inset Formula $AP=PJ.$
\end_inset

 La matriz P se obtiene hallando un par de vectores 
\begin_inset Formula $v_{1}$
\end_inset

 y 
\begin_inset Formula $v_{2}$
\end_inset

 LI que satisfagan las condiciones 
\begin_inset Formula $\left(A-\lambda I\right)v_{1}=0$
\end_inset

 y 
\begin_inset Formula $\left(A-\lambda I\right)v_{2}=v_{1}$
\end_inset

.
 Observamos que 
\begin_inset Formula $v_{1}$
\end_inset

 es autovector de 
\begin_inset Formula $A$
\end_inset

 asociado a 
\begin_inset Formula $\lambda$
\end_inset

.
\end_layout

\begin_layout Subsubsection
Caso 
\begin_inset Formula $\mathbf{A}\in\mathbb{R}^{\mathbf{3}\mathbf{\times}\mathbf{3}}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A$
\end_inset

 tiene un autovalor triple 
\begin_inset Formula $\lambda\in\mathbb{R}$
\end_inset

 de multiplicidad geométrica 1.
 En este caso: 
\begin_inset Formula 
\[
J=\left[\begin{array}{ccc}
\lambda & 1 & 0\\
0 & \lambda & 1\\
0 & 0 & \lambda
\end{array}\right]
\]

\end_inset

Respecto de 
\begin_inset Formula $P=[\begin{array}{ccc}
v_{1} & v_{2} & v_{3}]\end{array}$
\end_inset

, 
\begin_inset Formula $\{v_{1},v_{2},v_{3}\}$
\end_inset

 deben ser LI y satisfacer las condiciones 
\begin_inset Formula $\left(A-\lambda I\right)v_{1}=0,\left(A-\lambda I\right)v_{2}=v_{1},\left(A-\lambda I\right)v_{3}=v_{2}.$
\end_inset

 Observamos que 
\begin_inset Formula $v_{1}$
\end_inset

 es autovector de
\begin_inset Formula $A$
\end_inset

 asociado a 
\begin_inset Formula $\lambda$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $A$
\end_inset

 tiene un autovalor triple 
\begin_inset Formula $\lambda\in\mathbb{R}$
\end_inset

 de multiplicidad geométrica 2.
 En este caso: 
\begin_inset Formula 
\[
J=\left[\begin{array}{ccc}
\lambda & 1 & 0\\
0 & \lambda & 0\\
0 & 0 & \lambda
\end{array}\right]
\]

\end_inset

Respecto de 
\begin_inset Formula $P=[\begin{array}{ccc}
v_{1} & v_{2} & v_{3}]\end{array}$
\end_inset

, 
\begin_inset Formula $\{v_{1},v_{2},v_{3}\}$
\end_inset

 deben ser LI y satisfacer las condiciones 
\begin_inset Formula $\left(A-\lambda I\right)v_{1}=0,\left(A-\lambda I\right)v_{2}=v_{1},\left(A-\lambda I\right)v_{3}=0.$
\end_inset

 Observamos que 
\begin_inset Formula $v_{1}$
\end_inset

 y 
\begin_inset Formula $v_{3}$
\end_inset

 son autovectores de 
\begin_inset Formula $A$
\end_inset

 asociados a 
\begin_inset Formula $\lambda$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $A$
\end_inset

 tiene un autovalor doble 
\begin_inset Formula $\lambda\in\mathbb{R}$
\end_inset

 de multiplicidad geométrica 1 y un autovalor 
\begin_inset Formula $\mu\in\mathbb{R}$
\end_inset

 simple.
 En este caso 
\begin_inset Formula $J$
\end_inset

 debe tener dos bloques de Jordan: 
\begin_inset Formula 
\[
J=\left[\begin{array}{ccc}
\lambda & 1 & 0\\
0 & \lambda & 0\\
0 & 0 & \mu
\end{array}\right]
\]

\end_inset

Respecto de 
\begin_inset Formula $P=[\begin{array}{ccc}
v_{1} & v_{2} & v_{3}\end{array}]$
\end_inset

, 
\begin_inset Formula $\{v_{1},v_{2},v_{3}\}$
\end_inset

 deben ser LI y satisfacer las condiciones 
\begin_inset Formula $\left(A-\lambda I\right)v_{1}=0,\left(A-\lambda I\right)v_{2}=v_{1},\left(A-\mu I\right)v_{3}=0.$
\end_inset

 Observamos que 
\begin_inset Formula $v_{1}$
\end_inset

 y 
\begin_inset Formula $v_{3}$
\end_inset

 son autovectores de 
\begin_inset Formula $A$
\end_inset

 asociados a 
\begin_inset Formula $\lambda$
\end_inset

 y 
\begin_inset Formula $\mu$
\end_inset

 respectivamente.
\end_layout

