\begin_layout Subsection
Definiciones
\end_layout

\begin_layout Standard

\series bold
Ortogonalidad
\series default
: 
\begin_inset Formula $<x,y>=0\Longleftrightarrow x\bot y$
\end_inset

.
 
\end_layout

\begin_layout Standard

\series bold
Norma de un vector
\series default
 : 
\begin_inset Formula $\left|x\right|^{2}=<x,x>$
\end_inset

.
 La norma depende del producto interno.
\end_layout

\begin_layout Standard

\bar under
Propiedades de la norma
\bar default
: 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $|x|\in\mathbb{R}\,\forall x\in\mathbb{V}$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $|x|\geq0$
\end_inset

 (
\begin_inset Formula $|x|=0\Longleftrightarrow x=0$
\end_inset

) 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $|k\cdot x|=|k|\cdot|x|$
\end_inset

 
\end_layout

\begin_layout Enumerate
Desigualdad de Cauchy-Schwarz: 
\begin_inset Formula $|<x,y>|\leq\left|x\right|\cdot\left|y\right|$
\end_inset

, 
\begin_inset Formula $x,y\in\mathbb{V}_{\mathbb{K}}$
\end_inset

.
 Son iguales si 
\begin_inset Formula $x$
\end_inset

 es paralelo a 
\begin_inset Formula $y$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
Desigualdad triangular: 
\begin_inset Formula $\left|x+y\right|\leq\left|x\right|+\left|y\right|$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
Teorema de Pitágoras: si 
\begin_inset Formula $x\bot y$
\end_inset

 entonces 
\begin_inset Formula $\left|x+y\right|^{2}=\left|x\right|^{2}+\left|y\right|^{2}$
\end_inset

.
 La recíproca sólo vale para 
\begin_inset Formula $\mathbb{R}.$
\end_inset

 
\end_layout

\begin_layout Enumerate
Identidad del paralelogramo: 
\begin_inset Formula $\left|x+y\right|^{2}+\left|x-y\right|^{2}=2\left(\left|x\right|^{2}+\left|y\right|^{2}\right)$
\end_inset

, 
\begin_inset Formula $\forall x,y\in\mathbb{V}$
\end_inset

.
 
\end_layout

\begin_layout Standard

\series bold
Ángulo entre dos vectores
\series default
: 
\begin_inset Formula $\cos(\theta)=\frac{<x,y>}{|x|\cdot|y|}$
\end_inset

 con 
\begin_inset Formula $\theta\in[0,\pi]$
\end_inset

, 
\begin_inset Formula $\forall x,y\ne0,\forall$
\end_inset

 EPI real.
 
\end_layout

\begin_layout Standard

\series bold
Complemento ortogonal de un conjunto en un EPI
\series default
: Sea 
\begin_inset Formula $A\subset\mathbb{V}_{\mathbb{K}}$
\end_inset

.
 
\begin_inset Formula $A^{\bot}=\{x\in\mathbb{V}_{\mathbb{K}}:\,<x,y>=0\:\forall y\in A\}$
\end_inset

.
 Para el cálculo del complemento ortogonal a un subespacio de dimensión
 finita, alcanza con exigir la ortogonalidad a un sistema de generadores.
 
\end_layout

\begin_layout Standard

\series bold
Función distancia
\series default
: 
\begin_inset Formula $d:\mathbb{V}_{\mathbb{R}}\times\mathbb{V}_{\mathbb{R}}\rightarrow\mathbb{R}^{+}:d\left(x,y\right)=\left|x-y\right|=\left|y-x\right|$
\end_inset

 
\end_layout

\begin_layout Subsection
Matriz asociada a un PI
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $B=\{v_{1},v_{2},\ldots,v_{q}\}$
\end_inset

 base de 
\begin_inset Formula $\left(\mathbb{V}_{\mathbb{K}},\bullet\right)$
\end_inset

.
 Entonces 
\begin_inset Formula $G\in\mathbb{K}^{q\times q},g_{ij}=<v_{i},v_{j}>$
\end_inset

 es la matriz del producto interno: 
\begin_inset Formula 
\[
G=\left[\begin{array}{cccc}
\left|v_{1}\right|^{2} & \left(v_{1},v_{2}\right) & \cdots & \left(v_{1},v_{q}\right)\\
\left(v_{2},v_{1}\right) & \left|v_{2}\right|^{2} & \cdots & \left(v_{2},v_{q}\right)\\
\vdots & \vdots & \ddots & \vdots\\
\left(v_{q},v_{1}\right) & (v_{q},v_{2}) & \cdots & \left|v_{q}\right|^{2}
\end{array}\right]
\]

\end_inset

 
\bar under
Propiedades
\bar default
:
\end_layout

\begin_layout Itemize
\begin_inset Formula $g_{ii}>0$
\end_inset

 
\begin_inset Formula $\forall i=1,2,\ldots,q.$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $G^{H}=G$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $G$
\end_inset

 es definida positiva.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\exists\, G^{-1}$
\end_inset

.
 
\end_layout

\begin_layout Subsection
Expresión matricial de un PI
\end_layout

\begin_layout Standard
Si 
\begin_inset Formula $B$
\end_inset

 es base de 
\begin_inset Formula $\mathbb{V}_{\mathbb{K}}$
\end_inset

 y 
\begin_inset Formula $G$
\end_inset

 es la matriz del PI en esa base, entonces 
\begin_inset Formula $\forall x,y\in\mathbb{V}:$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
<x,y>=C_{B}^{H}(x)\cdot G\cdot C_{B}(y)
\]

\end_inset

 
\end_layout

\begin_layout Standard

\bar under
Propiedades
\bar default
:
\end_layout

\begin_layout Itemize
La matriz de un PI en una BOG es una matriz diagonal.
 
\end_layout

\begin_layout Itemize
La matriz de un PI en una BON es la matriz identidad.
 
\end_layout

\begin_layout Subsection
La mejor aproximación en EPIs
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $\mathbb{V}$
\end_inset

 un espacio vectorial de funciones, y 
\begin_inset Formula $\mathbb{W}$
\end_inset

 un subespacio de 
\begin_inset Formula $\mathbb{V}$
\end_inset

.
 Si se quiere aproximar 
\begin_inset Formula $f\in\mathbb{V}$
\end_inset

 con una función 
\begin_inset Formula $g\in\mathbb{W}$
\end_inset

, la mejor aproximación será la proyección ortogonal de 
\begin_inset Formula $f$
\end_inset

 sobre el subespacio 
\begin_inset Formula $\mathbb{W}$
\end_inset

.
\end_layout

\begin_layout Section
Proyecciones y Matrices de Proyección
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $S\subset\mathbb{V}$
\end_inset

 y 
\begin_inset Formula $S^{\bot}$
\end_inset

 su complemento ortogonal, entonces 
\begin_inset Formula $\forall x\in\mathbb{V}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
x\doteq\underset{\in S}{\underbrace{u}}+\underset{\in S^{\bot}}{\underbrace{v}}\doteq P_{S}(x)+P_{S^{\bot}}(x)
\]

\end_inset


\end_layout

\begin_layout Subsection
Propiedades de la proyección
\end_layout

\begin_layout Itemize
\begin_inset Formula $P_{S}\left(x\right)$
\end_inset

 es el vector de 
\begin_inset Formula $S$
\end_inset

 más próximo a 
\begin_inset Formula $x$
\end_inset

.
 
\end_layout

\begin_layout Itemize
\begin_inset Formula $P_{S}\left(v\right)=v\Longleftrightarrow v\in S$
\end_inset

 y además 
\begin_inset Formula $P_{S}\left(w\right)=0\Longleftrightarrow w\in S^{\bot}$
\end_inset

.
 
\end_layout

\begin_layout Itemize
Por Pitágoras: 
\begin_inset Formula $\left|x\right|^{2}=\left|P_{S}\left(x\right)\right|^{2}+\left|P_{S}^{\bot}\left(x\right)\right|^{2}\forall x\in\mathbb{V}$
\end_inset

.
 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left|P_{S}(x)\right|\leq\left|x\right|$
\end_inset

 (si 
\begin_inset Formula $\left|P_{S}(x)\right|=\left|x\right|$
\end_inset

 entonces 
\begin_inset Formula $x\in S$
\end_inset

) 
\end_layout

\begin_layout Itemize
\begin_inset Formula $d\left(x,S\right)=\left|P_{S^{\bot}}\left(x\right)\right|$
\end_inset

 
\end_layout

\begin_layout Subsection
Expresión de la proyección y la reflexión
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $S$
\end_inset

 un subespacio de 
\begin_inset Formula $\mathbb{V}$
\end_inset

, y 
\begin_inset Formula $B=\{v_{1},v_{2},\ldots,v_{q}\}$
\end_inset

 una BOG de 
\begin_inset Formula $S$
\end_inset

.
 Entonces 
\begin_inset Formula $\forall x\in\mathbb{V}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{S}(x)\doteq\sum_{i=1}^{q}\frac{<v_{i},x>}{<v_{i},v_{i}>}\cdot v_{i}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
R_{S}(x)=2P_{S}(x)-x=P_{S}(x)-P_{S^{\bot}}(x)=x-2P_{S^{\bot}}(x)
\]

\end_inset


\end_layout

\begin_layout Subsection
Proyecciones y TLs
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $T:\mathbb{V}_{\mathbb{K}}\rightarrow\mathbb{V}_{\mathbb{K}}$
\end_inset

 una transformación lineal tal que 
\begin_inset Formula $\left\{ \begin{array}{c}
Im\left(P_{S}\right)=S\\
Nu\left(P_{S}\right)=S^{\bot}
\end{array}\right.$
\end_inset

y sea 
\begin_inset Formula $B=\{\underbrace{v_{1},v_{2},\ldots,v_{q}}_{\in S},\underbrace{v_{q+1},v_{q+2},\ldots,v_{n}}_{\in S^{\bot}}\}$
\end_inset

 una base de 
\begin_inset Formula $\mathbb{V}$
\end_inset

, entonces la matriz de la TL es:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left[P_{S}\right]_{B}=\left[\begin{array}{cccccc}
1 &  &  &  & \cdots & 0\\
 & \ddots &  &  &  & \vdots\\
 &  & 1\\
 &  &  & 0\\
\vdots &  &  &  & \ddots\\
0 & \cdots &  &  &  & 0
\end{array}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
(tantos 1 como la dimensión del espacio sobre el cual proyecto, y tantos
 0 como la dimensión del complemento ortogonal)
\end_layout

\begin_layout Standard

\bar under
Nota
\bar default
: la matriz de un operador proyección en una BON es una matriz de proyección.
 En cualquier otra base, no lo es.
\end_layout

\begin_layout Subsection
Reflexiones y TLs
\end_layout

\begin_layout Standard
\begin_inset Wrap figure
lines 4
placement r
overhang 0in
width "4cm"
status collapsed

\begin_layout Plain Layout
\begin_inset Graphics
	filename proyeccion.png
	scale 50

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption

\begin_layout Plain Layout
Proyección y reflexión.
\end_layout

\end_inset


\end_layout

\end_inset

Sea 
\begin_inset Formula $T:\mathbb{V}_{\mathbb{K}}\to\mathbb{V}_{\mathbb{K}}$
\end_inset

 una TL tal que 
\begin_inset Formula $\left\{ \begin{array}{c}
T\left(v\right)=v,\forall v\in S\\
T\left(v\right)=-v,\forall v\in S^{\bot}
\end{array}\right.$
\end_inset

y sea 
\begin_inset Formula $B=\{\underbrace{v_{1},v_{2},\ldots,v_{q}}_{\in S},\underbrace{v_{q+1},v_{q+2},\ldots,v_{n}}_{\in S^{\bot}}\}$
\end_inset

 una base de 
\begin_inset Formula $\mathbb{V}$
\end_inset

, entonces la matriz de la TL es:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left[T\right]_{B}=\left[\begin{array}{cccccc}
1 &  &  &  & \cdots & 0\\
 & \ddots &  &  &  & \vdots\\
 &  & 1\\
 &  &  & -1\\
\vdots &  &  &  & \ddots\\
0 & \cdots &  &  &  & -1
\end{array}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
(tantos 
\begin_inset Formula $1$
\end_inset

 como la dimensión del espacio sobre el cual reflejo, y tantos 
\begin_inset Formula $-1$
\end_inset

 como la dimensión del complemento ortogonal)
\end_layout

\begin_layout Subsection
Matriz de Householder
\end_layout

\begin_layout Standard
La matriz de reflexión sobre un subespacio de dimensión 
\begin_inset Formula $n-1$
\end_inset

 que es ortogonal a un vector 
\begin_inset Formula $w$
\end_inset

 en un espacio de dimensión 
\begin_inset Formula $n$
\end_inset

 se puede obtener mediante la expresión: 
\begin_inset Formula 
\[
H=I_{d}-2\frac{ww^{T}}{w^{T}w}
\]

\end_inset


\bar under
Propiedades de la matriz de Householder: 
\end_layout

\begin_layout Itemize
Es involutiva: 
\begin_inset Formula $H\circ H=I_{d}$
\end_inset

.
 
\end_layout

\begin_layout Itemize
Es simétrica: 
\begin_inset Formula $H^{T}=H$
\end_inset

.
 
\end_layout

\begin_layout Itemize
Es inversible: 
\begin_inset Formula $\exists\, H^{-1}$
\end_inset

 y 
\begin_inset Formula $H^{-1}=H$
\end_inset

 
\end_layout

\begin_layout Itemize
Es ortogonal: 
\begin_inset Formula $H^{T}H=HH^{T}=Id$
\end_inset

.
 
\end_layout

\begin_layout Subsection
Rotaciones en 
\begin_inset Formula $\mathbb{R}^{\mathbf{3}}$
\end_inset


\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $B=\left\{ v_{1},v_{2},v_{3}\right\} $
\end_inset

 una BON de 
\begin_inset Formula $\mathbb{R}^{3}$
\end_inset

 y sea 
\begin_inset Formula $T$
\end_inset

 la rotación de 
\begin_inset Formula $\theta$
\end_inset

 grados alrededor del eje 
\begin_inset Formula $v_{1}$
\end_inset

.
 
\begin_inset Formula 
\[
\left[T\right]_{B}=\left[\begin{array}{ccc}
1 & 0 & 0\\
0 & \cos(\theta) & -\sin(\theta)\\
0 & \sin(\theta) & \cos(\theta)
\end{array}\right]
\]

\end_inset


\end_layout

\begin_layout Subsection
Proceso Gram-Schmidt
\end_layout

\begin_layout Standard
Dada una base 
\begin_inset Formula $\left\{ x_{1},x_{2},\ldots,x_{p}\right\} $
\end_inset

 para un subespacio 
\begin_inset Formula $\mathbb{W}$
\end_inset

 de 
\begin_inset Formula $\mathbb{R}^{n}$
\end_inset

, defina:
\end_layout

\begin_layout Itemize
\begin_inset Formula $v_{1}=x_{1}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $v_{2}=x_{2}-\frac{x_{2}.v_{1}}{v_{1}.v_{1}}\cdot v_{1}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $v_{p}=x_{p}-\frac{x_{p}\cdot v_{1}}{v_{1}\cdot v_{1}}\cdot v_{1}-\frac{x_{p}\cdot v_{2}}{v_{2}\cdot v_{2}}\cdot v_{2}-\ldots-\frac{x_{p}\cdot v_{p-1}}{v_{p-1}\cdot v_{p-1}}\cdot v_{p-1}$
\end_inset


\end_layout

\begin_layout Standard
Entonces 
\begin_inset Formula $\left\{ v_{1},v_{2},\ldots,v_{p}\right\} $
\end_inset

 es una BOG para 
\begin_inset Formula $\mathbb{W}$
\end_inset

.
\end_layout

\begin_layout Subsection
Matrices de Proyección
\end_layout

\begin_layout Standard
Trabajamos con el producto interno canónico y sobre 
\begin_inset Formula $\mathbb{K}^{n}$
\end_inset

, 
\begin_inset Formula $\mathbb{K}\text{=}\mathbb{R}$
\end_inset

 o 
\begin_inset Formula $\mathbb{C}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $P\in\mathbb{K}^{n\times n}$
\end_inset

 es una matriz de proyección 
\begin_inset Formula $\Longleftrightarrow\left\{ \begin{array}{c}
P^{2}=P\\
P^{H}=P
\end{array}\right.$
\end_inset


\end_layout

\begin_layout Standard

\bar under
Propiedades
\bar default
: 
\end_layout

\begin_layout Itemize
\begin_inset Formula $col(P)=nul(P)^{\bot}$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $P\cdot y=y\Longleftrightarrow y\in col(P)$
\end_inset


\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $P_{S}$
\end_inset

 es matriz de proyección sobre 
\begin_inset Formula $S$
\end_inset

 y 
\begin_inset Formula $P_{S^{\bot}}$
\end_inset

 es matriz de proyección sobre 
\begin_inset Formula $S^{\bot}$
\end_inset

 entonces 
\begin_inset Formula $P_{S}+P_{S^{\bot}}=Id$
\end_inset


\end_layout

\begin_layout Itemize
Las columnas de 
\begin_inset Formula $P$
\end_inset

 son una base del espacio sobre el cual proyectan.
 
\end_layout

\begin_layout Itemize
\begin_inset Formula $rg(P)=tr(P)$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\det(P)\neq0$
\end_inset

 si 
\begin_inset Formula $P\neq Id$
\end_inset


\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $P_{1}$
\end_inset

 y 
\begin_inset Formula $P_{2}$
\end_inset

 son matrices de proyección, y 
\begin_inset Formula $P_{1}\cdot P_{2}=P_{2}\cdot P_{1}=0$
\end_inset

, entonces 
\begin_inset Formula $P_{1}+P_{2}$
\end_inset

 es matriz de proyección y 
\begin_inset Formula $rg(P_{1}+P_{2})=rg(P_{1})+rg(P_{2})$
\end_inset


\end_layout

\begin_layout Standard
Obtención de la matriz de proyección:
\end_layout

\begin_layout Itemize
Sea 
\begin_inset Formula $Q$
\end_inset

 una matriz cuyas columnas son una BON de 
\begin_inset Formula $S\subset\mathbb{V}$
\end_inset

.
 Entonces la única matriz de proyección sobre 
\begin_inset Formula $S$
\end_inset

 es 
\begin_inset Formula $[P_{S}]=QQ^{T}$
\end_inset

.
 La matriz de proyección sobre 
\begin_inset Formula $S^{\bot}$
\end_inset

 es 
\begin_inset Formula $\left[P_{S}^{\bot}\right]=I_{d}-[P_{S}]$
\end_inset

 
\end_layout

\begin_layout Itemize
Sea 
\begin_inset Formula $B=\left\{ v_{1},v_{2},\ldots,v_{q}\right\} $
\end_inset

 una base de 
\begin_inset Formula $S$
\end_inset

, y 
\begin_inset Formula $A$
\end_inset

 la matriz que tiene por columnas a 
\begin_inset Formula $v_{1},v_{2},\ldots,v_{q}.$
\end_inset

 Entonces la única matriz de proyección sobre 
\begin_inset Formula $S$
\end_inset

 se obtiene mediante 
\begin_inset Formula $\left[P_{s}\right]=A\left(A^{H}A\right)^{-1}A^{H}=AA^{\#}$
\end_inset

 
\end_layout

\begin_layout Subsection
Inversas y pseudoinversas
\end_layout

\begin_layout Standard

\series bold
Pseudoinversa
\series default
: Sea 
\begin_inset Formula $A\in\mathbb{K}^{n\times q}$
\end_inset

 tal que 
\begin_inset Formula $rg\left(A\right)=q$
\end_inset

.
 La matriz pseudoinversa de 
\begin_inset Formula $A$
\end_inset

 es 
\begin_inset Formula $A^{\#}=(A^{H}A)^{-1}A^{H}$
\end_inset


\end_layout

\begin_layout Standard

\bar under
Propiedades
\bar default
: 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $A$
\end_inset

 es cuadrada e inversible, 
\begin_inset Formula $A^{-1}=A^{\#}$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $A^{\#}\in\mathbb{R}^{q\times n}$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $A^{\#}A=Id_{(q)}$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $AA^{\#}=[P]_{Col(A)}$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $Nul\left(AA^{\#}\right)=\left[Col\left(A\right)\right]^{\bot}$
\end_inset

 
\end_layout

\begin_layout Subsection
Cuadrados mínimos
\end_layout

\begin_layout Standard
\begin_inset Wrap figure
lines 8
placement r
overhang 0in
width "5cm"
status collapsed

\begin_layout Plain Layout
\begin_inset Graphics
	filename cuad_min.png
	scale 40

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption

\begin_layout Plain Layout
Cuadrados mínimos.
\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout

\end_layout

\end_inset

Sea 
\begin_inset Formula $A\in\mathbb{K}^{n\times q}$
\end_inset

, 
\begin_inset Formula $x\in\mathbb{K}^{q}$
\end_inset

, 
\begin_inset Formula $b\in\mathbb{R}^{n}$
\end_inset

.
 Si 
\begin_inset Formula $Ax=b$
\end_inset

 tiene una solución exacta, entonces 
\begin_inset Formula $b\in Col(A)$
\end_inset

.
 Si 
\begin_inset Formula $b\notin Col\left(A\right)$
\end_inset

, intentamos hallar una solución 
\begin_inset Formula $\hat{x}\in\mathbb{K}^{q}$
\end_inset

 (la solución por cuadrados mínimos) tal que: 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left|A\hat{x}-b\right|<\left|Au-b\right|\:\forall u\in\mathbb{K}^{q}$
\end_inset

, 
\end_layout

\begin_layout Itemize
\begin_inset Formula $d\left(A\hat{x},b\right)\le d(Au,b)\:\forall u\in\mathbb{K}^{q}$
\end_inset

, 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left|A\hat{x}\right|\le\left|b\right|$
\end_inset

 (son iguales si 
\begin_inset Formula $b\in Col\left(A\right)$
\end_inset

), 
\end_layout

\begin_layout Itemize
Ecuaciones normales de cuadrados mínimos: 
\begin_inset Formula $A^{T}A\hat{x}=A^{T}b$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $A\hat{x}=\hat{b}=P_{ColA}\left(b\right)\Longleftrightarrow\left\{ \begin{array}{c}
A\hat{x}\in Col(A)\\
b-A\hat{x}\in Col\left(A\right)^{\bot}
\end{array}\right.$
\end_inset

 
\end_layout

\begin_layout Standard
Observaciones: 
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $\hat{x}=0$
\end_inset

 entonces 
\begin_inset Formula $b\in\left[Col\left(A\right)\right]^{\bot}$
\end_inset

.
 La recíproca solo es cierta si 
\begin_inset Formula $A$
\end_inset

 es inversible.
 
\end_layout

\begin_layout Enumerate
Si las columnas de 
\begin_inset Formula $A$
\end_inset

 son LI, la solución por cuadrados mínimos es única y se obtiene mediante
 
\begin_inset Formula $\hat{x}=\left(A^{T}A\right)^{-1}A^{T}b=A^{\#}b$
\end_inset

.
 Si las columnas de 
\begin_inset Formula $A$
\end_inset

 son LD, el sistema 
\begin_inset Formula $A^{T}A\hat{x}=A^{T}b$
\end_inset

 tiene infinitas soluciones, y éstas son de la forma 
\begin_inset Formula $\hat{x}=\hat{x}_{p}+\underbrace{\hat{x}_{n}}_{\in Nul(A)}$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $b\in Col(A)$
\end_inset

, entonces toda solución de 
\begin_inset Formula $Ax=b$
\end_inset

 es una solución exacta y por cuadrados mínimos.
 
\end_layout

\begin_layout Enumerate
El error de aproximación 
\begin_inset Formula $\mathcal{E}$
\end_inset

 es igual a 
\begin_inset Formula $\left|b-\hat{b}\right|$
\end_inset

.
 
\end_layout

\begin_layout Subsection
Regresión lineal
\end_layout

\begin_layout Standard
Sean los puntos 
\begin_inset Formula $P_{i}=\left(x_{i},y_{i}\right)$
\end_inset

 con 
\begin_inset Formula $i=1,2,\ldots,n$
\end_inset

.
 La recta que mejor aproxima a los puntos es 
\begin_inset Formula $y=\alpha+\beta x$
\end_inset

, y los coeficientes 
\begin_inset Formula $\alpha,\beta$
\end_inset

 se obtienen resolviendo el sistema 
\begin_inset Formula $\left[\begin{array}{cc}
1 & x_{1}\\
1 & x_{2}\\
\vdots & \vdots\\
1 & x_{n}
\end{array}\right]\left(\begin{array}{c}
\alpha\\
\beta
\end{array}\right)=\left[\begin{array}{c}
\begin{array}{c}
y_{1}\\
y_{2}\\
\vdots
\end{array}\\
y_{n}
\end{array}\right]$
\end_inset

 por cuadrados mínimos.
\end_layout

\begin_layout Subsection
Solución por cuadrados mínimos de norma mínima
\end_layout

\begin_layout Standard
La solución por cuadrados mínimos de norma mínima pertenece al espacio 
\begin_inset Formula $Fil(A)$
\end_inset

 y se obtiene como 
\begin_inset Formula $\tilde{x}=A^{+}b$
\end_inset

, siendo 
\begin_inset Formula $A^{+}$
\end_inset

 la pseudoinversa de Moore-Penrose de 
\begin_inset Formula $A$
\end_inset

.
\end_layout

\begin_layout Section
Transformaciones Lineales
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $T\in\mathcal{L}(\mathbb{V}_{\mathbb{K}},\mathbb{W}_{\mathbb{K}})$
\end_inset

 y 
\begin_inset Formula $A=\left[T\right]_{BC}$
\end_inset

 con 
\begin_inset Formula $B$
\end_inset

 base de 
\begin_inset Formula $\mathbb{V}$
\end_inset

 y 
\begin_inset Formula $C$
\end_inset

 base de 
\begin_inset Formula $\mathbb{W}$
\end_inset

 la matriz de 
\begin_inset Formula $T.$
\end_inset


\end_layout

\begin_layout Subsection
Condiciones de TLs
\end_layout

\begin_layout Enumerate
\begin_inset Formula $T\left(u+v\right)=T\left(u\right)+T\left(v\right)$
\end_inset

 con 
\begin_inset Formula $u,v\in\mathbb{V}$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $T\left(\alpha\cdot u\right)=\alpha\cdot T\left(u\right)$
\end_inset

 con 
\begin_inset Formula $u\in\mathbb{V}_{\mathbb{K}}$
\end_inset

 y 
\begin_inset Formula $\alpha\in\mathbb{K}$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $T\left(0_{\mathbb{V}_{\mathbb{K}}}\right)=0_{\mathbb{W}_{\mathbb{K}}}.$
\end_inset

 
\end_layout

\begin_layout Subsection
Núcleo e imagen
\end_layout

\begin_layout Standard

\series bold
Núcleo
\series default
: 
\begin_inset Formula $Nu\left(T\right)=\left\{ v\in\mathbb{V}_{\mathbb{K}}:\, T\left(v\right)=0_{\mathbb{W}}\right\} =C_{B}^{-1}(Nul\left(A\right))$
\end_inset

.
 Es un subespacio.
 
\end_layout

\begin_layout Standard

\series bold
Imagen
\series default
: 
\begin_inset Formula $Im\left(T\right)=\left\{ w\in\mathbb{W}_{\mathbb{K}}:\, T\left(v\right)=w\text{ con }v\in\mathbb{V}_{\mathbb{K}}\right\} =C_{C}^{-1}(Col\left(A\right))$
\end_inset

.
 Es un subespacio.
 
\end_layout

\begin_layout Standard
La imagen de una TL puede obtenerse como lo que generan los transformados
 de una base del espacio de partida.
\end_layout

\begin_layout Standard

\series bold
Teorema de la dimensión
\series default
: Sea 
\begin_inset Formula $T\in\mathcal{L}(\mathbb{V},\mathbb{W})$
\end_inset

 y sea 
\begin_inset Formula $\dim\left(\mathbb{V}\right)=n$
\end_inset

 (finita).
 Entonces: 
\begin_inset Formula 
\[
\dim(\mathbb{V})=\dim\left(Nu(T)\right)+\dim\left(Im(T)\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Clasificación de TLs
\end_layout

\begin_layout Subsubsection
Inyectividad (monomorfismo)
\end_layout

\begin_layout Standard
Una TL es inyectiva si verifica: 
\begin_inset Formula $v_{1}\ne v_{2}\Longrightarrow T\left(v_{1}\right)\ne T\left(v_{2}\right)\forall v_{1},v_{2}\in\mathbb{V}$
\end_inset

.
\end_layout

\begin_layout Standard
Una TL es inyectiva 
\begin_inset Formula $\Longleftrightarrow$
\end_inset

 
\begin_inset Formula $Nu\left(T\right)=\left\{ 0_{\mathbb{V}}\right\} $
\end_inset

 
\begin_inset Formula $\Longleftrightarrow\dim\left(Im(T)\right)=\dim\left(\mathbb{V}\right)$
\end_inset


\end_layout

\begin_layout Standard
Una TL inyectiva transforma conjuntos LI a conjuntos LI.
 La recíproca también es cierta: si 
\begin_inset Formula $A$
\end_inset

 es un conjunto LI y es transformado en otro conjunto LI, la TL es inyectiva.
 Es decir: si 
\begin_inset Formula $T$
\end_inset

 es inyectiva y 
\begin_inset Formula $A$
\end_inset

 es LI, 
\begin_inset Formula $T\left(A\right)$
\end_inset

 es LI.
\end_layout

\begin_layout Standard
Las matrices asociadas a TLs inyectivas tienen sus columnas LI.
\end_layout

\begin_layout Standard
Si 
\begin_inset Formula $\dim(V)>\dim(\mathbb{W})$
\end_inset

, 
\begin_inset Formula $T$
\end_inset

 no puede ser inyectiva.
 
\end_layout

\begin_layout Subsubsection
Sobreyectividad (epimorfismo)
\end_layout

\begin_layout Standard
Una TL es sobreyectiva 
\begin_inset Formula $\Longleftrightarrow$
\end_inset

 
\begin_inset Formula $Im(T)=\mathbb{W}$
\end_inset

.
\end_layout

\begin_layout Standard
Las matrices asociadas a TLs sobreyectivas tienen sus filas LI.
\end_layout

\begin_layout Standard
Si 
\begin_inset Formula $\dim(\mathbb{W})>\dim(\mathbb{V})$
\end_inset

, 
\begin_inset Formula $T$
\end_inset

 no puede ser sobreyectiva.
 
\end_layout

\begin_layout Subsubsection
Biyectividad (isomorfismo)
\end_layout

\begin_layout Standard
Si 
\begin_inset Formula $\dim(\mathbb{W})=\dim(\mathbb{V})$
\end_inset

, y 
\begin_inset Formula $Nu\left(T\right)=\{0\}$
\end_inset

, 
\begin_inset Formula $T$
\end_inset

 es biyectiva.
\end_layout

\begin_layout Standard
\begin_inset Formula $T$
\end_inset

es biyectiva 
\begin_inset Formula $\Longleftrightarrow$
\end_inset

 si 
\begin_inset Formula $\{v_{1},\ldots,v_{n}\}$
\end_inset

 es base de 
\begin_inset Formula $\mathbb{V}$
\end_inset

, entonces 
\begin_inset Formula $\left\{ T\left(v_{1}\right),\ldots,T\left(v_{n}\right)\right\} $
\end_inset

 es base de 
\begin_inset Formula $\mathbb{W}$
\end_inset

.
\end_layout

\begin_layout Standard
La matriz asociada a una TL biyectiva tiene sus filas y columnas LI, o sea
 que es una matriz inversible, o sea que existe la TL inversa: 
\begin_inset Formula $T^{-1}=\left[T\right]^{-1}$
\end_inset


\end_layout

\begin_layout Standard
Si 
\begin_inset Formula $\dim\left(\mathbb{V}\right)=\dim(\mathbb{W})$
\end_inset

, entonces o bien 
\begin_inset Formula $T$
\end_inset

 es inyectiva y sobreyectiva, o bien no es ninguna.
\end_layout

\begin_layout Subsection
Matriz asociada a una TL
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $T\in\mathcal{L}(\mathbb{V}_{\mathbb{K}},\mathbb{W}_{\mathbb{K}})$
\end_inset

, sea 
\begin_inset Formula $B=\{v_{1},v_{2},\ldots,v_{q}\}$
\end_inset

 base de 
\begin_inset Formula $\mathbb{V}$
\end_inset

 y 
\begin_inset Formula $C=\{w_{1},w_{2},\ldots,w_{m}\}$
\end_inset

 base de 
\begin_inset Formula $\mathbb{W}$
\end_inset

.
 Entonces 
\begin_inset Formula $T$
\end_inset

 se puede escribir como 
\begin_inset Formula $T\left(x\right)=Ax$
\end_inset

, con 
\begin_inset Formula $A\in\mathbb{K}^{m\times q}$
\end_inset

 tal que
\begin_inset Formula $:$
\end_inset

 
\begin_inset Formula 
\[
A=\left[T\right]_{BC}=\left[\begin{array}{cccc}
| & | &  & |\\
C_{C}\left(T\left(v_{1}\right)\right) & C_{C}\left(T\left(v_{2}\right)\right) & \cdots & C_{C}(T\left(v_{q}\right))\\
| & | &  & |
\end{array}\right]
\]

\end_inset

 
\bar under
Propiedades
\bar default
: 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left[T\right]_{BC}\cdot C_{B}\left(v\right)=C_{C}\left(T\left(v\right)\right)\forall v\in\mathbb{V}$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $v\in Nu\left(T\right)\Longleftrightarrow C_{B}\left(v\right)\in Nul(A)$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $w\in Im\left(T\right)\Longleftrightarrow C_{C}\left(w\right)\in Col(A)$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\dim(Im\left(T\right))=rg(A)$
\end_inset

 
\end_layout

\begin_layout Standard

\series bold
Teorema
\series default
: sean 
\begin_inset Formula $\mathbb{V}$
\end_inset

 y 
\begin_inset Formula $\mathbb{W}$
\end_inset

 
\begin_inset Formula $\mathbb{K}$
\end_inset

-espacios vectoriales (
\begin_inset Formula $\mathbb{K}=\mathbb{R}\mbox{ ó }\mathbb{C}$
\end_inset

).
 Sea 
\begin_inset Formula $T:\mathbb{V}\to\mathbb{W}$
\end_inset

.
 Si 
\begin_inset Formula $B_{1}$
\end_inset

 y 
\begin_inset Formula $B_{2}$
\end_inset

 son bases ordenadas de 
\begin_inset Formula $\mathbb{V}$
\end_inset

, y 
\begin_inset Formula $C_{1}$
\end_inset

 y 
\begin_inset Formula $C_{2}$
\end_inset

 son bases ordenadas de 
\begin_inset Formula $\mathbb{W}$
\end_inset

, entonces 
\begin_inset Formula $rg\left(\left[T\right]_{B_{1}C_{1}}\right)=rg(\left[T\right]_{B_{2}C_{2}})$
\end_inset

.
\end_layout

\begin_layout Subsection
Teorema Fundamental de las TLs
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $B=\{v_{1},v_{2},\ldots,v_{n}\}$
\end_inset

 base de 
\begin_inset Formula $\mathbb{V}$
\end_inset

 y 
\begin_inset Formula $w_{1},w_{2},\ldots,w_{n}$
\end_inset

 vectores de 
\begin_inset Formula $\mathbb{W}.$
\end_inset

 Entonces existe y es única la TL que verifica que 
\begin_inset Formula $T\left(v_{1}\right)=w_{1}$
\end_inset

, 
\begin_inset Formula $T\left(v_{2}\right)=w_{2},\ldots,T\left(v_{n}\right)=w_{n}$
\end_inset

.
 Además, dada una TL y un par de bases, existe una única matriz asociada.
 La recíproca también es verdadera: dada una matriz y un par de bases, existe
 una única TL asociada.
\end_layout

\begin_layout Subsection
Composición de TLs
\end_layout

\begin_layout Standard
\begin_inset Wrap figure
lines 4
placement r
overhang 0in
width "5cm"
status collapsed

\begin_layout Plain Layout
\begin_inset Graphics
	filename composicion.png
	scale 40

\end_inset


\begin_inset Caption

\begin_layout Plain Layout
Composición.
\end_layout

\end_inset


\end_layout

\end_inset


\begin_inset Formula $f\in\mathcal{L}(\mathbb{V},\mathbb{W})$
\end_inset

 y 
\begin_inset Formula $g\in\mathcal{L}\left(\mathbb{W},\mathbb{H}\right)\Longrightarrow g\circ f\in\mathcal{L}\left(\mathbb{V},\mathbb{H}\right)$
\end_inset


\end_layout

\begin_layout Standard

\bar under
Propiedades
\bar default
: 
\end_layout

\begin_layout Itemize
\begin_inset Formula $Nu\left(f\right)\subseteq Nu\left(g\circ f\right)$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $Im\left(g\circ f\right)\subseteq Im(g)$
\end_inset

 
\end_layout

\begin_layout Subsection
Operadores lineales
\end_layout

\begin_layout Standard
Un operador lineal es una TL que va de un espacio en sí mismo.
 Se escribe como 
\begin_inset Formula $T\in\mathcal{L}(\mathbb{V})$
\end_inset

.
 
\end_layout

\begin_layout Standard

\bar under
Propiedades
\bar default
:
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $T_{1}\in\mathcal{L}(\mathbb{V})$
\end_inset

 y 
\begin_inset Formula $T_{2}\in\mathcal{L}(\mathbb{V})$
\end_inset

, entonces 
\begin_inset Formula $T_{1}\circ T_{2}\in\mathcal{L}(\mathbb{V})$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $T\in\mathcal{L}(\mathbb{V})$
\end_inset

, 
\begin_inset Formula $T^{n}=\underbrace{T\circ T\circ\ldots\circ T}_{n\: veces}$
\end_inset

 
\end_layout

\begin_layout Section
Autovalores y Autovectores
\end_layout

\begin_layout Subsection
Autovalores y autovectores de una matriz cuadrada
\end_layout

\begin_layout Standard

\series bold
Autovector
\series default
: Un vector 
\begin_inset Formula $v\ne\overline{0}$
\end_inset

 es autovector de 
\begin_inset Formula $A\in\mathbb{K}^{n\times n}$
\end_inset


\begin_inset Formula $\Longleftrightarrow\exists\lambda\in\mathbb{K}:\: Av=\lambda v$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
Autoespacio
\series default
: El autoespacio de 
\begin_inset Formula $A$
\end_inset

 asociado a un autovalor 
\begin_inset Formula $\lambda$
\end_inset

 es 
\begin_inset Formula $S_{\lambda}(A)=nul(A-\lambda I)$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
Polinomio característico
\series default
: El polinomio característico de una matriz 
\begin_inset Formula $A\in\mathbb{K}^{n\times n}$
\end_inset

 es 
\begin_inset Formula $p_{A}(\lambda)=\det(A-\lambda I)$
\end_inset

, y tiene grado 
\begin_inset Formula $n$
\end_inset

.
 Si 
\begin_inset Formula $\mathbb{K}=\mathbb{R}$
\end_inset

 el polinomio tiene a lo sumo 
\begin_inset Formula $n$
\end_inset

 raíces.
 Si 
\begin_inset Formula $\mathbb{K}=\mathbb{C}$
\end_inset

 tiene exactamente 
\begin_inset Formula $n$
\end_inset

 raíces.
\end_layout

\begin_layout Standard

\series bold
Autovalor
\series default
: Los autovalores de una matriz son las raíces de su polinomio característico.
\end_layout

\begin_layout Standard

\series bold
Espectro de una matriz
\series default
: 
\begin_inset Formula $\sigma\left(A\right)=\{\lambda\in\mathbb{K}:\:\lambda\mbox{es autovalor de \ensuremath{A\}}}$
\end_inset


\end_layout

\begin_layout Subsection
Multiplicidad geométrica y algebraica de un autovalor
\end_layout

\begin_layout Standard
\begin_inset Formula $m_{g}(\lambda)=\dim\left(S_{\lambda}(A)\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $m_{a}(\lambda)=$
\end_inset

 número de veces que aparece 
\begin_inset Formula $\lambda$
\end_inset

 como raíz del polinomio característico.
\end_layout

\begin_layout Standard
Siempre se verifica que: 
\begin_inset Formula $1\le m_{g}\left(\lambda\right)\le m_{a}\left(\lambda\right)$
\end_inset

.
\end_layout

\begin_layout Subsection
Propiedades
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $A\in\mathbb{K}^{n\times n}.$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $A$
\end_inset

 es singular 
\begin_inset Formula $\Longleftrightarrow0$
\end_inset

 es un autovalor de 
\begin_inset Formula $A$
\end_inset

 
\begin_inset Formula $\Longleftrightarrow m_{g}\left(0\right)=n-k\Longleftrightarrow rg\left(A\right)=k<n$
\end_inset

 .
 
\end_layout

\begin_layout Itemize
Dos autovectores asociados a autovalores distintos son LI.
 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $A\in\mathbb{K}^{2\times2}$
\end_inset

, entonces 
\begin_inset Formula $p_{A}\left(\lambda\right)=\lambda^{2}-tr\left(A\right)\lambda+\det(A)$
\end_inset

.
 
\end_layout

\begin_layout Itemize
Si todas las filas o columnas de 
\begin_inset Formula $A$
\end_inset

 suman 
\begin_inset Formula $s$
\end_inset

 entonces 
\begin_inset Formula $s\in\sigma(A)$
\end_inset

.
 
\end_layout

\begin_layout Itemize
Sea 
\begin_inset Formula $p\left(t\right)=a_{k}t^{k}+\ldots+a_{1}t+a_{0}\,(a_{k}\ne0)$
\end_inset

.
 Si 
\begin_inset Formula $\lambda$
\end_inset

 es autovalor de 
\begin_inset Formula $A$
\end_inset

, entonces se cumple que 
\begin_inset Formula $p\left(\lambda\right)$
\end_inset

 es autovalor de 
\begin_inset Formula $p\left(A\right)$
\end_inset

, y para cada autovalor 
\begin_inset Formula $\mu$
\end_inset

 de 
\begin_inset Formula $p(A)$
\end_inset

 existe un autovalor 
\begin_inset Formula $\lambda$
\end_inset

 de 
\begin_inset Formula $A$
\end_inset

 tal que 
\begin_inset Formula $p\left(\lambda\right)=\mu$
\end_inset

.
 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $\lambda$
\end_inset

 es autovalor de 
\begin_inset Formula $A$
\end_inset

...
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $\lambda$
\end_inset

 es autovalor de 
\begin_inset Formula $A^{T}$
\end_inset

, 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lambda^{-1}$
\end_inset

 es autovalor de 
\begin_inset Formula $A^{-1}$
\end_inset

 y 
\begin_inset Formula $S_{\lambda^{-1}}\left(A^{-1}\right)=S_{\lambda}\left(A\right)$
\end_inset

, 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $r\cdot\lambda$
\end_inset

 es autovalor de 
\begin_inset Formula $r\cdot A$
\end_inset

, 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lambda^{k}$
\end_inset

 es autovalor de 
\begin_inset Formula $A^{k}$
\end_inset

, 
\begin_inset Formula $k\in\mathbb{N}$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lambda+r$
\end_inset

 es autovalor de 
\begin_inset Formula $A+r\cdot I$
\end_inset

.
 
\end_layout

\end_deeper
\begin_layout Subsection
Autovalores y autovectores de operadores lineales
\end_layout

\begin_layout Standard
\begin_inset Formula $T:\mathbb{V}_{\mathbb{K}}\to\mathbb{V}_{\mathbb{K}}.$
\end_inset

 Un vector 
\begin_inset Formula $v\ne\overline{0}$
\end_inset

 es autovector de 
\begin_inset Formula $T$
\end_inset

 
\begin_inset Formula $\Longleftrightarrow T\left(v\right)=\lambda v,$
\end_inset

 con 
\begin_inset Formula $\lambda$
\end_inset

 autovalor de 
\begin_inset Formula $T$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $S_{\lambda}\left(T\right)=\left\{ x\in\mathbb{V}:\: T\left(x\right)=\lambda x\mbox{ y }\lambda\text{ autovalor de }T\right\} =Nuc(T-\lambda I).$
\end_inset

 Si 
\begin_inset Formula $B$
\end_inset

 es base de 
\begin_inset Formula $\mathbb{V}$
\end_inset

 y 
\begin_inset Formula $A$
\end_inset

 es la matriz de 
\begin_inset Formula $T$
\end_inset

 en esa base, entonces: 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\sigma\left(A\right)=\sigma\left(T\right)\,\forall B$
\end_inset

 base de 
\begin_inset Formula $\mathbb{V}$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $x$
\end_inset

 es autovector de
