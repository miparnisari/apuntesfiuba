\end_inset


\end_layout

\begin_layout Standard
En 
\begin_inset Formula $C^{n\times m}:\,<A,B>=tr(A^{H}B)$
\end_inset


\end_layout

\begin_layout Standard
En 
\begin_inset Formula $P_{R}[a,b]:\,<p,q>=\int_{a}^{b}p(t)g(t)\, dt$
\end_inset


\end_layout

\begin_layout Standard
En 
\begin_inset Formula $P_{C}[a,b]:\,<p,q>=\int_{a}^{b}\overline{p(t)}g(t)\, dt$
\end_inset


\end_layout

\begin_layout Standard
En 
\begin_inset Formula $C_{R}[a,b]:\,<f,g>=\int_{a}^{b}f(t)g(t)\, dt$
\end_inset


