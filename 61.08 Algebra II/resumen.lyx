\end_layout

\begin_layout Subsection
Axiomas
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $<\bullet,\bullet>:\mathbb{V}_{\mathbb{K}}\times\mathbb{V}_{\mathbb{K}}\to\mathbb{R}$
\end_inset

 un producto interno.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $<x,y>\in\mathbb{K}$
\end_inset

 y 
\begin_inset Formula $\forall x,y\in\mathbb{V}$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $<x,y>=<\overline{y,x}>$
\end_inset

 
\begin_inset Formula $\forall x,y\in\mathbb{V}$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $<\lambda x,y>=\overline{\lambda}<x,y>$
\end_inset

 
\begin_inset Formula $\forall x,y\in\mathbb{V}$
\end_inset

 y 
\begin_inset Formula $\forall\lambda\in\mathbb{K}$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $<x,\lambda y>=\lambda<x,y>$
\end_inset

 
\begin_inset Formula $\forall x,y\in\mathbb{V}$
\end_inset

 y 
\begin_inset Formula $\forall\lambda\in\mathbb{K}$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $<x,y+z>=<x,y>+<x,z>$
\end_inset

 
\begin_inset Formula $\forall x,y,z\in\mathbb{V}$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $<x,x>=0$
\end_inset

 
\begin_inset Formula $\forall x\in\mathbb{V}$
\end_inset

 (si 
\begin_inset Formula $<x,x>=0$
\end_inset

 entonces 
\begin_inset Formula $x=0$
\end_inset

) 
\end_layout

\begin_layout Subsection
Productos internos canónicos (PIC)
\end_layout

\begin_layout Standard
En 
\begin_inset Formula $R^{n}:\,<x,y>=x^{T}y$
\end_inset


\end_layout

\begin_layout Standard
En 
\begin_inset Formula $C^{n}:\,<x,y>=x^{H}y$
\end_inset


\end_layout

\begin_layout Standard
En 
