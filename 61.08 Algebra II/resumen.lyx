\begin_layout Subsection
Diagonalización
\end_layout

\begin_layout Standard
\begin_inset Formula $A\in\mathbb{K}^{n\times n}$
\end_inset

 es diagonalizable 
\begin_inset Formula $\Longleftrightarrow A\sim D\Longleftrightarrow$
\end_inset

 existe una base de 
\begin_inset Formula $\mathbb{K}^{n}$
\end_inset

 compuesta por autovectores de 
\begin_inset Formula $A\Longleftrightarrow A$
\end_inset

 tiene 
\begin_inset Formula $n$
\end_inset

 autovectores LI 
\begin_inset Formula $\Longleftrightarrow m_{g}\left(\lambda\right)=m_{a}\left(\lambda\right)\:\forall\lambda\in\sigma(A)\Longleftrightarrow\exists\, P$
\end_inset

 inversible y 
\begin_inset Formula $D$
\end_inset

 diagonal tal que: 
\begin_inset Formula $A=PDP^{-1}$
\end_inset

, siendo 
\begin_inset Formula $P$
\end_inset

 la matriz de autovectores y 
\begin_inset Formula $D$
\end_inset

 la matriz de autovalores.
\end_layout

\begin_layout Subsubsection
Matrices trivialmente diagonalizables
\end_layout

\begin_layout Itemize
Nula: autovalor 0, autovectores: cualquier vector no nulo.
 
\end_layout

\begin_layout Itemize
Identidad: autovalor: 1, autovectores: cualquier vector no nulo.
 
\end_layout

\begin_layout Itemize
Diagonal (
\begin_inset Formula $A=\left(\begin{array}{ccc}
\alpha_{1} & 0 & 0\\
0 & \ddots & 0\\
0 & 0 & \alpha_{n}
\end{array}\right))$
\end_inset

: autovalores: 
\begin_inset Formula $\alpha_{1},\ldots,\alpha_{n}$
\end_inset

, autovectores: los que tienen sus componentes nulas excepto la n-ésima.
 
\end_layout

\begin_layout Itemize
Escalar (
\begin_inset Formula $A=\left(\begin{array}{ccc}
k & 0 & 0\\
0 & \ddots & 0\\
0 & 0 & k
\end{array}\right),k\in\mathbb{R}$
\end_inset

): autovalor: 
\begin_inset Formula $k$
\end_inset

, autovectores: cualquier vector no nulo.
 
\end_layout

\begin_layout Itemize
De proyección (
\begin_inset Formula $A\in\mathbb{K}^{n\times n},\dim S=q)$
\end_inset

: autovalor 1 con 
\begin_inset Formula $m_{a}\left(1\right)=m_{g}\left(1\right)=q$
\end_inset

, autovalor 0 con 
\begin_inset Formula $m_{a}\left(0\right)=m_{g}\left(0\right)=n-q$
\end_inset

 
\end_layout

\begin_layout Itemize
De reflexión: autovalor 1 con 
\begin_inset Formula $m_{a}\left(1\right)=q$
\end_inset

, autovalor -1 con 
\begin_inset Formula $m_{a}\left(0\right)=n-q$
\end_inset

 
\end_layout

\begin_layout Subsubsection
Propiedades
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $A$
\end_inset

 es diagonalizable entonces 
\begin_inset Formula $A^{n}$
\end_inset

 es diagonalizable (
\begin_inset Formula $D_{A}^{n}=D_{A^{n}}$
\end_inset

 y 
\begin_inset Formula $\sigma(A^{n})=\sigma^{n}(A)$
\end_inset

).
 La recíproca es falsa.
 
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $A\in\mathbb{C}^{n\times n}$
\end_inset

 tiene 
\begin_inset Formula $n$
\end_inset

 autovalores distintos entonces 
\begin_inset Formula $A$
\end_inset

 es diagonalizable.
 La recíproca es falsa.
 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\det\left(A\right)=\prod_{i=1}^{n}\lambda_{i}=p_{A}(0)$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left(A\right)=\sum_{i=1}^{n}\lambda_{i}$
\end_inset

 
\end_layout

\begin_layout Subsection
Autovalores complejos de una matriz real
\end_layout

\begin_layout Standard
Supongamos que 
\begin_inset Formula $\lambda=a+bi$
\end_inset

 con 
\begin_inset Formula $a,b\in\mathbb{R}$
\end_inset

, 
\begin_inset Formula $b\ne0$
\end_inset

 es un autovalor de 
\begin_inset Formula $A\in\mathbb{R}^{n\times n}$
\end_inset

.
 Entonces 
\begin_inset Formula $\overline{\lambda}=a-bi$
\end_inset

 también es autovalor de 
\begin_inset Formula $A$
\end_inset

 y 
\begin_inset Formula $v\in\mathbb{C}^{n}$
\end_inset

 es autovector de 
\begin_inset Formula $A$
\end_inset

 asociado a 
\begin_inset Formula $\lambda$
\end_inset

 si y sólo si 
\begin_inset Formula $\overline{v}$
\end_inset

 es autovector de A asociado a 
\begin_inset Formula $\overline{\lambda}$
\end_inset

.
 En particular, si 
\begin_inset Formula $\{v_{1},\ldots,v_{r}\}$
\end_inset

 es base de 
\begin_inset Formula $S_{\lambda}$
\end_inset

 entonces 
\begin_inset Formula $\{\overline{v}_{1},\ldots,\overline{v_{r}}\}$
\end_inset

 es base de 
\begin_inset Formula $S_{\overline{\lambda}}$
\end_inset

.
\end_layout

\begin_layout Subsection
Diagonalización de TLs
\end_layout

\begin_layout Standard
\begin_inset Formula $T\in\mathcal{L}(\mathbb{V}_{\mathbb{K}})$
\end_inset

 con 
