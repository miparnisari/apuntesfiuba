\begin_layout Standard
El 
\series bold
método de las diferencias divididas de Newton
\series default
 consiste en definir
\end_layout

\begin_layout Itemize
\begin_inset Formula $f(x_{i})=y_{i}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $f(x_{i},x_{i+1})=\frac{f(x_{i+1})-f(x_{i})}{x_{i+1}-x_{i}}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $f(x_{i},x_{i+1},x_{i+2})=\frac{f(x_{i+1},x_{i+2})-f(x_{i},x_{i+1})}{x_{i+2}-x_{i}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vdots$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $f(x_{k},\ldots,x_{n})=\frac{f(x_{k+1},\ldots,x_{n})-f(x_{k},\ldots,x_{n-1})}{x_{n}-x_{k}}$
\end_inset


\end_layout

