\end_layout

\begin_layout Standard

\series bold
Regla de Bayes
\series default
: si los eventos 
\begin_inset Formula $B_{1},...,B_{k}$
\end_inset

 constituyen una partición del espacio muestral 
\begin_inset Formula $\Omega$
\end_inset

 tal que 
\begin_inset Formula $P\left(B_{i}\right)\neq0$
\end_inset

 para 
\begin_inset Formula $i=1,...,k$
\end_inset

, entonces para cualquier evento A de 
\begin_inset Formula $\Omega$
\end_inset

 tal que 
\begin_inset Formula $P(A)\neq0$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P\left(B_{r}|A\right)=\frac{P(B_{r}\cap A)}{\sum_{i=1}^{k}P\left(B_{i}\cap A\right)}=\frac{P\left(B_{r}\right)\cdot P\left(A|B_{r}\right)}{\sum_{i=1}^{k}P\left(B_{i}\right)\cdot P\left(A|B_{i}\right)}
\]

\end_inset


\end_layout

