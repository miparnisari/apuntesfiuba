\begin_layout Section
Variables aleatorias y distribuciones de probabilidad
\end_layout

\begin_layout Subsection
Concepto de variable aleatoria
\end_layout

\begin_layout Standard

\series bold
Variable aleatoria
\series default
: función que asocia un número real con cada elemento del espacio muestral.
 Tiene la forma 
\begin_inset Formula $f_{X}(x):\Omega\rightarrow\Re$
\end_inset

, donde 
\begin_inset Formula $X$
\end_inset

 denota la variable aleatoria, 
\begin_inset Formula $x$
\end_inset

 uno de sus valores, y 
\begin_inset Formula $\Omega$
\end_inset

 el espacio muestral.
 Hay de tres tipos:
\end_layout

\begin_layout Itemize

\series bold
Discreta
\series default
: hay finitos resultados posibles, o infinitos numerables resultados posibles.
 Representan datos por conteo.
\end_layout

\begin_layout Itemize

\series bold
Continua
\series default
: su conjunto de valores posibles es un intervalo de números.
 Representan datos medidos (pesos, alturas, distancias, etc.)
\end_layout

\begin_layout Itemize

\series bold
Mixta
\series default
.
\end_layout

\begin_layout Standard

\emph on
Teorema
\emph default
: 
\begin_inset Formula $X$
\end_inset

 e 
\begin_inset Formula $Y$
\end_inset

 son variables aleatorias independientes sí y sólo si 
\begin_inset Formula $f(x,y)=f(x)f(y)\,\forall(x,y)$
\end_inset

.
\end_layout

\begin_layout Standard

\emph on
Condición necesaria (pero no suficiente) para que 
\begin_inset Formula $X$
\end_inset

,
\begin_inset Formula $Y$
\end_inset

 sean independientes
\emph default
: el soporte de 
\begin_inset Formula $f(x,y)$
\end_inset

 debe ser un rectángulo o un conjunto ordenado de ellos.
\end_layout

\begin_layout Subsection
Distribuciones discretas de probabilidad
\end_layout

\begin_layout Standard

\series bold
Función de probabilidad de una variable aleatoria discreta 
\begin_inset Formula $X$
\end_inset


\series default
: conjunto de pares ordenados 
\begin_inset Formula $\left(x,P(X=x)\right)$
\end_inset

 tales que para cada resultado posible 
\begin_inset Formula $x$
\end_inset

:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $P\left(X=x\right)\geq0$
\end_inset

,
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\sum_{\forall x}P\left(X=x\right)=1$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
Función de distribución acumulada
\series default
 
\series bold
de una variable aleatoria discreta 
\begin_inset Formula $X$
\end_inset


\series default
: dada la distribución de probabilidad 
\begin_inset Formula $P(X=x)$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
F_{X}(x)=P(X\leq x)=\sum_{t\leq x}P(X=t)\mbox{ para \ensuremath{-\infty<x<\infty}}
\]

\end_inset


\end_layout

\begin_layout Standard
Propiedades de la función de distribución:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $P(a<X\leq b)=F_{X}(b)-F_{X}(a)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $P(a\leq X\leq b)=F_{X}(b)-F_{X}(a)+P(a)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $P(a\leq X<b)=F_{X}(b)-F_{X}(a)+P(a)-P(b)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $P(a<X<b)=F_{X}(b)-F_{X}(a)-P(b)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $F_{X}(x)$
\end_inset

 es no decreciente y continua a derecha
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lim_{x\to-\infty}F_{X}(x)=0$
\end_inset

; 
\begin_inset Formula $\lim_{x\to+\infty}F_{X}(x)=1$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Átomo de 
\begin_inset Formula $X$
\end_inset


\series default
: se dice que 
\begin_inset Formula $a\in\Re$
\end_inset

 es un átomo de la variable aleatoria discreta 
\begin_inset Formula $X$
\end_inset

 si 
\begin_inset Formula $P\left(X=a\right)>0$
\end_inset

.
\end_layout

\begin_layout Subsection
Distribuciones continuas de probabilidad
\end_layout

\begin_layout Standard

\series bold
Función de densidad de probabilidad de una variable aleatoria continua 
\begin_inset Formula $X$
\end_inset


\series default
: función 
\begin_inset Formula $f(x)$
\end_inset

 definida en el conjunto 
\begin_inset Formula $\Re$
\end_inset

, tal que:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $f(x)\geq0$
\end_inset

 para toda 
\begin_inset Formula $x\in\Re$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\int_{-\infty}^{\infty}f(x)\, dx=1$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $P(X=x)=0$
\end_inset

 para toda 
\begin_inset Formula $x\in\Re$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $P\left(a<X<b\right)=P\left(a\leq X\leq b\right)=\int_{a}^{b}f(x)\, dx$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Función de distribución acumulada de una variable aleatoria continua 
\begin_inset Formula $X$
\end_inset


\series default
: dada la función de densidad 
\begin_inset Formula $f(x)$
\end_inset

, se define a la función de distribución como
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
F_{X}(x)=P(X\leq x)=\int_{-\infty}^{x}f(t)\, dt\mbox{ para }-\infty<x<\infty
\]

\end_inset


\end_layout

\begin_layout Subsection
Distribuciones mixtas de probabilidad
\end_layout

\begin_layout Standard
\begin_inset Formula $X$
\end_inset

 es una variable aleatoria 
\series bold
mixta
\series default
 cuando:
\end_layout

\begin_layout Itemize
Existen 
\begin_inset Formula $a$
\end_inset

 tales que 
\begin_inset Formula $P\left(X=a\right)>0$
\end_inset

.
 Dichos puntos 
\begin_inset Formula $a$
\end_inset

 son los 
\series bold
puntos pesados
\series default
 de 
\begin_inset Formula $X$
\end_inset

, es decir, que acumulan probabilidad.
\end_layout

\begin_layout Itemize
\begin_inset Formula $P\left(a\leq X\leq b\right)=\left[\int_{a}^{b}f_{X}(x)\, dx\right]+P(X=a)+P(X=b)$
\end_inset


\end_layout

\begin_layout Subsection
Distribuciones de probabilidad conjunta
\end_layout

\begin_layout Subsubsection
Caso discreto
\end_layout

\begin_layout Standard

\series bold
Función de probabilidad conjunta
\series default
: si 
\begin_inset Formula $X$
\end_inset

 e 
\begin_inset Formula $Y$
\end_inset

 son dos variables aleatorias discretas, la distribución de probabilidad
 para sus ocurrencias simultáneas se representa mediante una función 
\begin_inset Formula $P(x,y)$
\end_inset

 que verifica:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $P(x,y)=P\left[\left(X=x\right)\cap\left(Y=y\right)\right]$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $P(x,y)\geq0\mbox{ para todo }(x,y)$
\end_inset

 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\sum_{\forall x}\sum_{\forall y}P(x,y)=1$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Función de probabilidad marginal
\series default
: 
\begin_inset Formula $\begin{cases}
P\left(X=x\right)=\sum_{\forall y}\, P(x,y)\\
P\left(Y=y\right)=\sum_{\forall x}\, P(x,y)
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Función de probabilidad condicional
\series default
: 
\begin_inset Formula $\begin{cases}
P(Y=y|X=x)=\frac{P(x,y)}{P\left(X=x\right)}\\
P(X=x|Y=y)=\frac{P(x,y)}{P\left(Y=y\right)}
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Función de probabilidad conjunta
\series default
: 
\begin_inset Formula $P(x,y)=P(x)\cdot P(y|x)=P(y)\cdot P(x|y)$
\end_inset


\end_layout

\begin_layout Subsubsection
Caso continuo
\end_layout

\begin_layout Standard

\series bold
Función de densidad conjunta
\series default
: la función 
\begin_inset Formula $f(x,y)$
\end_inset

 es una función de densidad conjunta de las variables aleatorias continuas
 
\begin_inset Formula $X$
\end_inset

 y 
\begin_inset Formula $Y$
\end_inset

 si:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $f(x,y)\geq0$
\end_inset

 para todo 
\begin_inset Formula $(x,y)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\int_{-\infty}^{+\infty}\int_{-\infty}^{+\infty}\, f(x,y)\, dx\, dy=1$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $P[(X,Y)\in A]=\iint_{A}\, f(x,y)\, dx\, dy$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Función de probabilidad marginal
\series default
: 
\begin_inset Formula $\begin{cases}
f_{X}(x)=\int_{-\infty}^{+\infty}\, f(x,y)\, dy\\
f_{Y}(y)=\int_{-\infty}^{+\infty}\, f(x,y)\, dx
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Función de probabilidad condicional
\series default
: 
\begin_inset Formula $\begin{cases}
f_{Y|X}(y|x)=\frac{f(x,y)}{f_{X}(x)}\\
f_{X|Y}(x|y)=\frac{f(x,y)}{f_{Y}(y)}
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Función de probabilidad conjunta
\series default
: 
\begin_inset Formula $f(x,y)=f(x)\cdot f(y|x)=f(y)\cdot f(x|y)$
\end_inset


\end_layout

\begin_layout Subsection
Variables truncadas
\end_layout

\begin_layout Standard

\series bold
Truncado
\series default
: truncar la variable aleatoria 
\begin_inset Formula $X$
\end_inset

 a un conjunto 
\begin_inset Formula $B\subset R$
\end_inset

 significa condicionarla a tomar valores en el conjunto 
\begin_inset Formula $B$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
F_{X|X\in B}(x)=P(X\leq x|X\in B)=\frac{P(X\leq x,X\in B)}{P(X\in B)}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f_{X|X\in B}(x)=\frac{f_{X}(x)\mathbf{1}\{x\in B\}}{P(X\in B)}
\]

\end_inset


\end_layout

\begin_layout Subsection
Mezcla de variables aleatorias
\end_layout

\begin_layout Standard
Sean 
\begin_inset Formula $n$
\end_inset

 variables aleatorias 
\begin_inset Formula $X_{i}$
\end_inset

, donde cada una se asocia a un resultado 
\begin_inset Formula $R_{i}$
\end_inset

 de un experimento aleatorio.
 Si 
\begin_inset Formula $P\left(R_{i}\right)$
\end_inset

 es la probabilidad de ocurrencia de cada resultado, y 
\begin_inset Formula $\sum_{i=1}^{n}P\left(R_{i}\right)=1$
\end_inset

, entonces:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f_{X_{M}}(x)=f_{X_{1}}(x)\cdot P\left(R_{1}\right)+f_{X_{2}}(x)\cdot P\left(R_{2}\right)+\cdots+f_{X_{n}}(x)\cdot P\left(R_{n}\right)
\]

\end_inset


\end_layout

\begin_layout Standard

\bar under
Propiedades
\bar default
:
\end_layout

\begin_layout Itemize
\begin_inset Formula $E\left[X_{M}\right]=\sum_{i=1}^{n}E\left[X_{i}\right]\cdot P\left(R_{i}\right)$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\sigma_{X_{M}}^{2}=\sum_{i=1}^{n}P\left(R_{i}\right)\cdot\left[\sigma_{X_{i}}^{2}+\left(E\left[X_{i}\right]-E\left[X_{M}\right]\right)^{2}\right]$
\end_inset


\end_layout

\begin_layout Subsection
Suma aleatoria de variables aleatorias
\end_layout

\begin_layout Standard
Dada una cantidad aleatoria 
\begin_inset Formula $Y$
\end_inset

 de términos 
\begin_inset Formula $X_{i}$
\end_inset

 idénticamente distribuidos, y sea la variable aleatoria 
\begin_inset Formula 
\[
W|Y=\sum_{i=1}^{Y}X_{i}
\]

\end_inset

 Entonces:
\end_layout

\begin_layout Itemize
\begin_inset Formula $E(W)=E(Y)\cdot E(X)$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\sigma^{2}(W)=E(Y)\sigma^{2}(X)+E^{2}(X)\sigma^{2}(Y)$
\end_inset


\end_layout

