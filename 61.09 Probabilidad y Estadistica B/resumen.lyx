\begin_layout Part
Estadística
\end_layout

\begin_layout Standard

\emph on
Objetivo
\emph default
: a partir de las observaciones de un fenómeno aleatorio, hacer inferencias
 sobre la distribución de probabilidades subyacente.
 
\end_layout

\begin_layout Section
Definiciones
\end_layout

\begin_layout Standard

\series bold
Población
\series default
: totalidad de las observaciones en las que estamos interesados, de número
 finito o infinito.
\end_layout

\begin_layout Standard

\series bold
Muestra
\series default
: subconjunto de una población.
\end_layout

\begin_layout Standard

\series bold
Muestra aleatoria
\series default
: sean 
\begin_inset Formula $X_{1},X_{2},\ldots,X_{n}$
\end_inset

 variables aleatorias independientes, cada una con la misma distribución
 de probabilidad 
\begin_inset Formula $f_{X}(x)$
\end_inset

.
 Definimos 
\begin_inset Formula $X_{1},X_{2},\ldots,X_{n}$
\end_inset

 como una muestra aleatoria de tamaño 
\begin_inset Formula $n$
\end_inset

 de la población 
\begin_inset Formula $f_{X}(x)$
\end_inset

, y su distribución de probabilidad conjunta es 
\begin_inset Formula $f\left(x_{1},x_{2},\ldots,x_{n}\right)=f\left(x_{1}\right)f\left(x_{2}\right)\cdots f\left(x_{n}\right)$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
Estadístico
\series default
: variable aleatoria que es función de variables aleatorias que forman una
 muestra aleatoria.
\end_layout

\begin_layout Standard

\series bold
Estimador
\series default
: estadístico que se usa para estimar el valor de un parámetro desconocido
 de una distribución de probabilidad.
\end_layout

\begin_layout Section
Estadística inferencial
\end_layout

\begin_layout Subsection
Propiedades de los estimadores
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $X$
\end_inset

 una variable aleatoria que depende de un parámetro desconocido 
\begin_inset Formula $\theta$
\end_inset

.
 Sea 
\begin_inset Formula $\hat{\theta}$
\end_inset

 un estimador de 
\begin_inset Formula $\theta$
\end_inset

 basado en una muestra aleatoria de tamaño 
\begin_inset Formula $n$
\end_inset

.
\end_layout

\begin_layout Enumerate

\series bold
Sesgo
\series default
: 
\begin_inset Formula $B\left[\hat{\theta}\right]=E\left[\hat{\theta}\right]-\theta$
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Si 
\begin_inset Formula $B\left[\hat{\theta}\right]=0$
\end_inset

 para el parámetro 
\begin_inset Formula $\theta$
\end_inset

, el estimador 
\begin_inset Formula $\hat{\theta}$
\end_inset

 es 
\series bold
insesgado
\series default
.
\end_layout

\begin_layout Itemize
Si 
\begin_inset Formula $\lim_{n\to\infty}B\left[\hat{\theta}\right]=0$
\end_inset

, el estimador 
\begin_inset Formula $\hat{\theta}$
\end_inset

 es 
\series bold
asintóticamente insesgado
\series default
, y por lo tanto, 
\begin_inset Formula $ECM\left[\hat{\theta}\right]=var\left[\hat{\theta}\right]$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate

\series bold
Varianza
\series default
: 
\begin_inset Formula $var\left[\hat{\theta}\right]=E\left[(\hat{\theta}-\theta)^{2}\right]$
\end_inset


\end_layout

\begin_layout Enumerate

\series bold
Error cuadrático medio
\series default
:
\begin_inset Formula 
\begin{eqnarray*}
ECM\left[\hat{\theta}\right] & = & E\left[\left(\hat{\theta}-\theta\right)^{2}\right]\\
 & = & var\left[\hat{\theta}\right]+B^{2}\left[\hat{\theta}\right]
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Enumerate

\series bold
Consistencia
\series default
: a medida que aumenta el tamaño de la muestra, el estimador se aproxima
 al verdadero valor del parámetro.
\end_layout

\begin_deeper
\begin_layout Enumerate

\series bold
Consistencia débil
\series default
: para cada 
\begin_inset Formula $\epsilon>0$
\end_inset

, debe cumplir que
\begin_inset Formula 
\[
\lim_{n\to\infty}P\left(\left|\hat{\theta}-\theta\right|>\epsilon\right)=0
\]

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard

\emph on
Teorema
\emph default
: si 
\begin_inset Formula $\hat{\theta}$
\end_inset

 es asintóticamente insesgado y 
\begin_inset Formula $var\left[\hat{\theta}\right]\underset{n\rightarrow\infty}{\longrightarrow}0$
\end_inset

, entonces 
\begin_inset Formula $\hat{\theta}$
\end_inset

 es débilmente consistente.
\end_layout

\end_deeper
\begin_layout Enumerate

\series bold
Consistencia fuerte:
\series default

\begin_inset Formula 
\[
P\left(\lim_{n\to\infty}\hat{\theta}=\theta\right)=1
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate

\series bold
Eficiencia
\series default
: si 
\begin_inset Formula $var\left[\hat{\theta}_{1}\right]<var\left[\hat{\theta}_{2}\right]$
\end_inset

 entonces 
\begin_inset Formula $\hat{\theta}_{1}$
\end_inset

 es mejor estimador que 
\begin_inset Formula $\hat{\theta}_{2}$
\end_inset

.
\end_layout

\begin_layout Subsection
Comparación de estimadores
\end_layout

\begin_layout Standard
El estimador 
\begin_inset Formula $\hat{\theta}_{1}$
\end_inset

 es mejor que el estimador 
\begin_inset Formula $\hat{\theta}_{2}$
\end_inset

 si 
\begin_inset Formula $ECM\left[\hat{\theta}_{1}\right]\leq ECM\left[\hat{\theta}_{2}\right]$
\end_inset

 para todo 
\begin_inset Formula $\theta$
\end_inset

, con desigualdad estricta para al menos un valor de 
\begin_inset Formula $\theta$
\end_inset

.
\end_layout

\begin_layout Subsection
Algunos estadísticos importantes
\end_layout

\begin_layout Standard
Sean 
\begin_inset Formula $X_{1},X_{2},\ldots,X_{n}$
\end_inset

 una muestra aleatoria de tamaño 
\begin_inset Formula $n$
\end_inset

 de una población con distribución 
\begin_inset Formula $X\sim\mathcal{N}\left(\mu,\sigma^{2}\right)$
\end_inset

.
\end_layout

\begin_layout Itemize
Media de la muestra:
\begin_inset Formula 
\[
\bar{X}=\frac{1}{n}\sum_{i=1}^{n}X_{i}
\]

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Es insesgado.
\end_layout

\begin_layout Itemize
\begin_inset Formula $ECM\left[\bar{X}\right]=\frac{\sigma^{2}}{n}$
\end_inset


\end_layout

\begin_layout Itemize
Distribución muestral de 
\begin_inset Formula $\bar{X}$
\end_inset

: 
\begin_inset Formula $\bar{X}\sim\mathcal{N}\left(\mu_{\bar{X}}=\mu,\sigma_{\bar{X}}^{2}=\frac{\sigma^{2}}{n}\right)$
\end_inset

.
\end_layout

\begin_deeper
\begin_layout Standard
(Si 
\begin_inset Formula $X\not\sim\mathcal{N}(\mu,\sigma^{2})$
\end_inset

, la distribución anterior vale sólo para 
\begin_inset Formula $n$
\end_inset

 suficientemente grande).
\end_layout

\end_deeper
\end_deeper
\begin_layout Itemize
Varianza de la muestra:
\begin_inset Formula 
\begin{eqnarray*}
S^{2} & = & \frac{1}{n-1}\sum_{i=1}^{n}\left(X_{i}-\bar{X}\right)^{2}\\
 & = & \frac{1}{n\left(n-1\right)}\left[n\sum_{i=1}^{n}X_{i}^{2}-\left(\sum_{i=1}^{n}X_{i}\right)^{2}\right]
\end{eqnarray*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Es insesgado.
\end_layout

\begin_layout Itemize

\emph on
Teorema
\emph default
: si 
